	<parameter name="payment_status" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT
    SPA.PAYADJUST_DOCNO,
    SPA.MEMBER_NO,
    SPA.PAYADJUST_DATE,
    SPA.SHARESTK_VALUE,
    SPA.SHRLAST_PERIOD,
    SPA.PERIODBASE_VALUE,
    SPA.OLD_PERIODVALUE,
    SPA.OLD_PAYSTATUS,
    SPA.NEW_PERIODVALUE,
    SPA.NEW_PAYSTATUS,
    SPA.REMARK,
    SPA.ENTRY_ID,
    SPA.ENTRY_DATE,
    SPA.CHGSTOP_FLAG,
    SPA.CHGLOW_FLAG,
    ft_getmbname(SPA.COOP_ID,SPA.MEMBER_NO) as fullname,
    MM.SALARY_AMOUNT,
    MM.MEMBGROUP_CODE,
    MG.MEMBGROUP_DESC,
    CC.COOP_NAME,
    SPA.SHRPAYADJ_STATUS,
    SPA.CHGADD_FLAG,
    SPA.CHGLOW_FLAG,
    SPA.CHGSTOP_FLAG,
    SPA.CHGCONT_FLAG,
    MT.MEMBTYPE_CODE,
    MT.MEMBTYPE_DESC,
    MP.PROVINCE_DESC,
    MG.MEMBGROUP_CODE
FROM SHPAYMENTADJUST SPA
LEFT JOIN MBMEMBMASTER MM       ON SPA.MEMBER_NO      = MM.MEMBER_NO
LEFT JOIN MBUCFPRENAME PR       ON MM.PRENAME_CODE    = PR.PRENAME_CODE
LEFT JOIN MBUCFMEMBGROUP MG     ON MM.MEMBGROUP_CODE  = MG.MEMBGROUP_CODE
LEFT JOIN CMCOOPCONSTANT CC     ON SPA.COOP_ID        = CC.COOP_ID
LEFT JOIN SHSHAREMASTER SSM     ON SPA.MEMBER_NO      = SSM.MEMBER_NO
LEFT JOIN SHSHARETYPE SST       ON SSM.SHARETYPE_CODE = SST.SHARETYPE_CODE
LEFT JOIN MBUCFMEMBTYPE MT      ON MM.MEMBTYPE_CODE   = MT.MEMBTYPE_CODE
LEFT JOIN MBUCFPROVINCE MP      ON MP.PROVINCE_CODE   = MG.PROVINCE_CODE
WHERE
    SPA.COOP_ID = $P{as_coop}
    AND (
        $P{ai_status} IS NULL
        OR SPA.SHRPAYADJ_STATUS = $P{ai_status}
    )
    AND (
        SPA.PAYADJUST_DATE BETWEEN $P{adtm_start}
        AND (TRUNC($P{adtm_end}) + 1)
    )
    AND (
            ($P{payment_status} = '01'
                AND SPA.new_paystatus = 1
                AND SPA.new_periodvalue > SPA.old_periodvalue)

        OR  ($P{payment_status} = '02'
                AND SPA.new_paystatus = 1
                AND SPA.new_periodvalue < SPA.old_periodvalue)

        OR  ($P{payment_status} = '03'
                AND SPA.new_paystatus = -1)

        OR  ($P{payment_status} NOT IN ('01','02','03')
                AND SPA.new_paystatus = 1
                AND SPA.old_paystatus = -1)
    )
ORDER BY SPA.PAYADJUST_DATE, SPA.MEMBER_NO]]>
	</queryString>
