	<queryString>
		<![CDATA[SELECT mbucfprename.prename_desc||mbmembmaster.memb_name||'  '||mbmembmaster.memb_surname as member_name,
MBUCFMEMBGROUP.MEMBGROUP_CODE,
MBUCFMEMBGROUP.MEMBGROUP_DESC,
DPDEPTMASTER.MEMBER_NO,
MBMEMBMASTER.MEMBGROUP_CODE,
DPDEPTMASTER.DEPTACCOUNT_NO,
DPDEPTMASTER.DEPTTYPE_CODE,
DPDEPTMASTER.DEPTACCOUNT_NAME,
DPDEPTMASTER.DEPTMONTH_AMT
FROM DPDEPTMASTER
inner join MBMEMBMASTER on DPDEPTMASTER.MEMBER_NO=MBMEMBMASTER.MEMBER_NO
inner join MBUCFMEMBGROUP on MBMEMBMASTER.MEMBGROUP_CODE=MBUCFMEMBGROUP.MEMBGROUP_CODE
inner join MBUCFPRENAME on MBUCFPRENAME.PRENAME_CODE=MBMEMBMASTER.PRENAME_CODE
WHERE
DPDEPTMASTER.COOP_ID = $P{as_coopid} AND
DPDEPTMASTER.DEPTCLOSE_STATUS=0 AND
DPDEPTMASTER.DEPTMONTH_STATUS=1 AND
DPDEPTMASTER.DEPTMONTH_AMT > 0
order by MBUCFMEMBGROUP.MEMBGROUP_CODE,DPDEPTMASTER.MEMBER_NO,DPDEPTMASTER.DEPTACCOUNT_NO]]>
	</queryString>
