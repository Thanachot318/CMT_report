	<queryString>
		<![CDATA[select		mbp.prename_desc || hr.emp_name || ' ' || hr.emp_surname as full_name,
hra.seq_no,
			hra.emp_no,
			hra.assist_name,
			hra.assist_posit,
			hra.assist_amp,
			hra.assist_age,
			hrd.education_desc,
			hra.assist_month,
			hra.assist_docno_date,
			hra.assist_begin,
			hra.assist_amt,
			hra.assist_use,
			hra.sum_balance,
			hra.assist_remark,
			hs.pos_desc,
			$P{as_sdate} as sdate,
			$P{as_edate} as edate
from 	hremployeeassist hra,
		mbucfprename mbp,
		hremployee hr,
		hrucfeducation hrd,
		hrucfpositiondet hd ,
		hrucfpositionmas hs
where hr.prename_code = mbp.prename_code
and  trim(hr.emp_no) = trim(hra.emp_no(+))
and  hra.education_code = hrd.education_code(+)
and hr.pos_code = hd.pos_code(+)
and hd.pos_mas = hs.pos_mas(+)
and hra.assist_code = '02'
and hra.apv_status = 1
and hra.coop_id =$P{as_coopid}
and trim(hra.emp_no) between trim ($P{as_sempno}) and trim($P{as_eempno})
and TRUNC(hra.assist_docno_date) between TRUNC($P{as_sdate}) and TRUNC($P{as_edate})
order by  hra.assist_docno_date,hra.emp_no, hra.seq_no]]>
	</queryString>
