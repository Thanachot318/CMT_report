	<queryString>
		<![CDATA[SELECT
    sh.member_no,
    ft_getmbname(sh.coop_id, sh.member_no) AS fullname,
    mg.MEMBGROUP_DESC ,
    mp.PROVINCE_DESC ,
    CASE
        WHEN sh.payment_status = '1' THEN 'ปกติ'
        WHEN sh.payment_status = '-1' THEN 'งดส่ง'
        ELSE ''
    END AS payment_desc,
    sh.periodshare_amt * shtype.UNITSHARE_VALUE as periodshare,
    sh.SHARESTK_VALUE
FROM shsharemaster sh
LEFT JOIN MBMEMBMASTER mb ON mb.member_no = SH.MEMBER_NO
LEFT JOIN MBUCFMEMBGROUP mg ON mb.MEMBGROUP_CODE = mg.MEMBGROUP_CODE
LEFT JOIN MBUCFPROVINCE mp ON mp.PROVINCE_CODE = mg.PROVINCE_CODE
LEFT JOIN SHSHARETYPE shtype ON shtype.SHARETYPE_CODE = sh.SHARETYPE_CODE
WHERE sharemaster_status = 1
order by member_no]]>
	</queryString>
