	<queryString>
		<![CDATA[select
mbmembmaster.member_no,
ft_getmbname(mbmembmaster.coop_id,mbmembmaster.member_no)as fullname ,
mbmembmaster.membgroup_code,
mbucfmembgroup.membgroup_desc ,
TRUNC(MONTHS_BETWEEN(SYSDATE, mbmembmaster.birth_date) / 12) || ' ปี ' || TRUNC(MOD(MONTHS_BETWEEN(SYSDATE, mbmembmaster.birth_date), 12)) || ' เดือน' as age ,
ft_getmbaddr(mbmembmaster.coop_id,mbmembmaster.member_no, 1) || ' โทรศัพท์ ' || mbmembmaster.mem_tel as addr,
mbmembmaster.member_date,
mbmembmaster.resign_date,
mbucfmembtype.membtype_desc,
mbmembmaster.salary_amount,
mbucfposition.position_desc,
mbucfprovince.province_desc,
(case when mariage_status = 1 then 'โสด'
when mariage_status = 2 then 'สมรส'
when mariage_status = 3 then 'หย่าร้าง'
when mariage_status = 4 then 'หม้าย' else '' end ) as mariage_status

from mbmembmaster
left join mbucfmembgroup on mbmembmaster.membgroup_code = mbucfmembgroup.membgroup_code
left join mbucfmembtype on mbmembmaster.membtype_code = mbucfmembtype.membtype_code
left join mbucfposition on mbmembmaster.position_code = mbucfposition.position_code
left join mbucfprovince on mbucfmembgroup.province_code = mbucfprovince.province_code
where mbmembmaster.member_no = $P{as_memno}]]>
	</queryString>
