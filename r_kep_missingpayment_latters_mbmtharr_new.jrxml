	<queryString>
		<![CDATA[select ln.member_no, ft_getmbname(ln.coop_id,ln.member_no)as fullname, calint_to,
ln.loancontract_no, ln.loanapprove_amt, ft_readtbaht(ln.loanapprove_amt)as loanappr_read, ln.period_payamt, ln.period_payment, ln.last_periodpay, ln.principal_balance, ft_readtbaht(ln.principal_balance)as prnbal_read
,(ln.loanapprove_amt-ln.principal_balance)as pay ,ft_readtbaht(ln.loanapprove_amt-ln.principal_balance)as pay_read, ln.loanapprove_date, cm.coop_name, cm.manager, cm.addr_no||' หมู่'||cm.addr_moo||' ถนน'||cm.addr_road||' แขวง'||cm.addr_tambol||' เขต'||cm.addr_amphur||' '||cm.addr_province||' '||cm.addr_postcode as coop_addr
from lncontmaster ln
join cmcoopmaster cm on ln.coop_id = cm.coop_id
join mbmembmtharrear ma on ma.coop_id = ln.coop_id and ma.loancontract_no = ln.loancontract_no
join mbmembmaster mm on mm.member_no = ln.member_no
where ln.contract_status = 1
and ln.coop_id = $P{coop_id}
and ma.recv_period = $P{recv_period}
and mm.member_no between $P{smember_no} and $P{emember_no}
and mm.membgroup_code between $P{smembgroup_code} and $P{emembgroup_code}]]>
	</queryString>
