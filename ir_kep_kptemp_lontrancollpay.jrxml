	<queryString>
		<![CDATA[select kt.member_no
, kt.recv_period
, ktd.loancontract_no
, ktd.principal_payment
, ktd.interest_payment
, ktd.item_payment
, mb.memb_name
, mb.memb_surname
, mbtrn.memb_name as mbtrnmemb_name
, mbtrn.memb_surname as mbtrnmemb_surname
, kt.coop_id
, cm.coop_name
, mgc.membgroup_control
, mgc.membgroup_controldesc
from kptempreceive kt
join kptempreceivedet ktd on kt.coop_id = ktd.coop_id and kt.kpslip_no = ktd.kpslip_no and ktd.keepitem_status = 1
join lncontmaster lm on ktd.coop_id = lm.coop_id and ktd.loancontract_no = lm.loancontract_no and lm.transfer_status = 1
join mbmembmaster mb on lm.coop_id = mb.coop_id and lm.member_no = mb.member_no
join mbmembmaster mbtrn on lm.coop_id = mbtrn.coop_id and lm.trnfrom_memno = mbtrn.member_no
join cmcoopmaster cm on kt.coop_id = cm.coop_id
join mbucfmembgroup mbg on kt.coop_id = mbg.coop_id and kt.membgroup_code = mbg.membgroup_code
join mbucfmembgroupcontrol mgc on mbg.coop_id = mgc.coop_id and mbg.membgroup_control = mgc.membgroup_control
where kt.coop_id = $P{as_coopid}
and kt.recv_period = $P{as_recvperiod}
and mgc.membgroup_control between $P{as_smembgroup} and $P{as_emembgroup}
order by mgc.membgroup_control asc]]>
	</queryString>
