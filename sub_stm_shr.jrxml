	<queryString>
		<![CDATA[select
shsharestatement.slip_date ,
trim(shsharestatement.ref_slipno) as ref_slipno,
shsharestatement.shritemtype_code ,
shsharestatement.period ,
shsharestatement.share_amount * nvl(shsharetype.unitshare_value,10) as share_value ,
shsharestatement.share_amount ,
sharestk_amt * nvl(shsharetype.unitshare_value,10) as share_balance
from shsharestatement
left join shsharetype on shsharestatement.sharetype_code = shsharetype.sharetype_code
where shsharestatement.member_no = $P{member_no}
and shsharestatement.slip_date between $P{s_date} and $P{e_date}
order by shsharestatement.member_no,shsharestatement.seq_no]]>
