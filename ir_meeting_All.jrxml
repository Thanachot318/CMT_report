	<queryString>
		<![CDATA[SELECT
    CMCOOPCONSTANT.COOP_NAME,
    CMCOOPCONSTANT.COOP_ID,
    mtmeeting.meeting_id,
    mtmeeting.topic,
    mtmeeting.meeting_start as MEETING_START,
    mtmeeting.meeting_end as MEETING_END,
    mtmeeting.staff_id,
    mtmeeting.location
FROM
    mtmeeting
left join CMCOOPCONSTANT on CMCOOPCONSTANT.COOP_ID = mtmeeting.COOP_ID
WHERE
    CMCOOPCONSTANT.COOP_ID = $P{as_coopid}
    AND MEETING_START between $P{as_startdate} and (TRUNC($P{as_enddate})+1)
ORDER BY
    mtmeeting.meeting_id]]>
	</queryString>
