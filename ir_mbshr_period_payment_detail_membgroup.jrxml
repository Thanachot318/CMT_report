	<queryString>
		<![CDATA[SELECT
    shsharemaster.member_no,
    ft_getmbname(shsharemaster.coop_id, shsharemaster.member_no) AS fullname,
    CASE
        WHEN shsharemaster.payment_status = '1' THEN 'ปกติ'
        WHEN shsharemaster.payment_status = '-1' THEN 'งดส่ง'
        ELSE ''
    END AS payment_desc,
    shsharemaster.periodshare_amt,
mbmembmaster.membgroup_code,
mbucfmembgroup.membgroup_desc,
mbucfprovince.province_desc,
shsharemaster.SHARESTK_VALUE
FROM shsharemaster
left join mbmembmaster on shsharemaster.member_no = mbmembmaster.member_no
left join mbucfmembtype on mbmembmaster.membtype_code = mbucfmembtype.membtype_code
left join mbucfmembgroup on mbmembmaster.membgroup_code = mbucfmembgroup.membgroup_code
left join mbucfprovince on mbucfmembgroup.province_code = mbucfprovince.province_code
WHERE shsharemaster.sharemaster_status = 1 order by mbmembmaster.membgroup_code,
shsharemaster.member_no]]>
	</queryString>
