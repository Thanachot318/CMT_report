	<queryString>
		<![CDATA[select lncollmaster.*,
		cmcoopmaster.*,
		MBMEMBMASTER.*,
		( MBUCFPRENAME.PRENAME_DESC || MBMEMBMASTER.MEMB_NAME || '   ' || MBMEMBMASTER.MEMB_SURNAME  ) as Mem_Name,
		mut.tambol_desc as tambol_desc2,
		mud.district_desc as amphur_desc2,
		mup.province_desc as province_desc2,
		FT_READTBAHT(est_price) as m_bath, 0 as dividend_flag, 0 as average_flag, 0 as ivr_password
from lncollmaster
left join cmcoopmaster on cmcoopmaster.coop_id = lncollmaster.coop_Id
left join MBMEMBMASTER on lncollmaster.member_no = MBMEMBMASTER.member_no
left join MBUCFPRENAME on MBMEMBMASTER.PRENAME_CODE = MBUCFPRENAME.PRENAME_CODE
left outer join mbucftambol mut on lncollmaster.mrtg_tambol1 = mut.tambol_code and lncollmaster.mrtg_amphur1 = mut.district_code
left outer join mbucfdistrict mud on lncollmaster.mrtg_amphur1 = mud.district_code and lncollmaster.mrtg_province1 = mud.province_code
left outer join mbucfprovince mup on lncollmaster.mrtg_province1 = mup.province_code
where lncollmaster.collmast_no= $P{colmast_no} and lncollmaster.coop_id = $P{coopid}]]>
	</queryString>
