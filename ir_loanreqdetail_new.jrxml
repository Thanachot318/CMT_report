	<queryString>
		<![CDATA[SELECT
lnreqloan.LOANREQUEST_DOCNO ,
    FT_GETGRPNAME(lnreqloan.coop_id, mbmembmaster.membgroup_code) AS grp_desc,
    lnreqloan.LOANREQUEST_DATE AS LOANREQUEST_DATE,
    lnreqloan.MEMBER_NO AS MEMBER_NO,
    NULL AS coll_desc,
    FT_GETMBNAME(lnreqloan.coop_id, lnreqloan.member_no) AS name,
    mbmembmaster.SALARY_AMOUNT AS SALARY_AMOUNT,
    lnreqloan.INCOMEMONTH_OTHER AS INCOMEMONTH_OTHER,
    lnreqloan.SHARE_BALANCE AS SHARE_BALANCE,
    lnreqloan.LOANTYPE_CODE AS LOANTYPE_CODE,
    lnreqloan.LOANPERMISS_AMT AS LOANPERMISS_AMT,
    lnreqloan.LOANREQUEST_AMT AS LOANREQUEST_AMT,
    NULL AS loan_clr,
    CMUCFMONEYTYPE.MONEYTYPE_DESC AS MONEYTYPE_DESC,
    lnreqloan.RECVESTIMATE_AMT AS RECVESTIMATE_AMT,
    lnreqloan.ENTRY_ID AS ENTRY_ID
FROM
    lnreqloan
LEFT JOIN mbmembmaster ON lnreqloan.member_no = mbmembmaster.member_no
LEFT JOIN CMUCFMONEYTYPE ON lnreqloan.EXPENSE_CODE = CMUCFMONEYTYPE.MONEYTYPE_CODE

where lnreqloan.LOANREQUEST_DATE between  $P{adtm_start} and $P{adtm_end}
and lnreqloan.LOANTYPE_CODE between $P{sloantype_code} and $P{eloantype_code}
and lnreqloan.loanrequest_status like $P{status}]]>
	</queryString>
