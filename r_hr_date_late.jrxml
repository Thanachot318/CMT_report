	</parameter>
	<queryString>
		<![CDATA[SELECT
  he.emp_no,
  mp.prename_desc || he.emp_name || '  ' || he.emp_surname AS emp_name,

  -- ลากิจ (04)
  SUM(CASE WHEN hl.leave_code = '04' THEN NVL(hl.totalday, 0) ELSE 0 END) AS leave1,
  COUNT(CASE WHEN hl.leave_code = '04' THEN hl.leave_from ELSE NULL END) AS leave12,

  -- ลาป่วย (01)
  SUM(CASE WHEN hl.leave_code = '01' THEN NVL(hl.totalday, 0) ELSE 0 END) AS leave2,
  COUNT(CASE WHEN hl.leave_code = '01' THEN hl.leave_from ELSE NULL END) AS leave22,

  -- รวมลากิจและลาป่วย
  SUM(CASE WHEN hl.leave_code BETWEEN '04' AND '02' THEN NVL(hl.totalday, 0) ELSE 0 END) AS leave3,
  COUNT(CASE WHEN hl.leave_code BETWEEN '04' AND '02' THEN hl.leave_from ELSE NULL END) AS leave32,

  -- ลาอบรม (06)
  SUM(CASE WHEN hl.leave_code = '06' THEN NVL(hl.totalday, 0) ELSE 0 END) AS leave6,
  COUNT(CASE WHEN hl.leave_code = '06' THEN hl.leave_from ELSE NULL END) AS leave62,

  -- ลาอื่น (14)
  SUM(CASE WHEN hl.leave_code = '14' THEN NVL(TO_NUMBER(hl.totaltime), 0) ELSE 0 END) AS leave4,
  COUNT(CASE WHEN hl.leave_code = '14' THEN hl.leave_from ELSE NULL END) AS leave42,
  TO_NUMBER(SUBSTR(TO_CHAR(SUM(CASE WHEN hl.leave_code = '14' THEN NVL(TO_NUMBER(hl.totaltime), 0) ELSE 0 END), '999.99'), -2)) AS leave43,

  -- มาสาย
  COUNT(DISTINCT hr.late_date) AS late
FROM hremployee he
JOIN mbucfprename mp ON he.prename_code = mp.prename_code

-- ดึง leave ทั้งหมดในช่วงเวลาที่กำหนด
LEFT JOIN hrlogleave hl ON hl.emp_no = he.emp_no 
  AND hl.leave_from BETWEEN $P{adtm_start} AND $P{adtm_end}

-- ดึง late log
LEFT JOIN hrloglate hr ON hr.emp_no = he.emp_no
  AND hr.late_date BETWEEN $P{adtm_start} AND $P{adtm_end}

WHERE he.emp_status = 1
  AND he.emptype_code = '01'
  AND he.coop_id = $P{as_coopid}

GROUP BY he.emp_no, mp.prename_desc, he.emp_name, he.emp_surname

ORDER BY he.emp_no
]]>
	</queryString>
