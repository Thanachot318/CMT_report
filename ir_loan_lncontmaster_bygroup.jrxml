	<queryString>
		<![CDATA[SELECT
    g.membgroup_code,
    mp.membgroup_desc,
    NVL(sa.share_balance, 0)      AS share_balance,
    NVL(la.principal_balance, 0)  AS principal_balance,
    NVL(la.loanapprove_amt, 0)    AS loanapprove_amt,
    NVL(la.interest_arrear, 0)    AS interest_arrear,
    NVL(g.accum_interest,0) AS accum_interest
FROM (
    SELECT coop_id, membgroup_code, SUM(accum_interest) AS accum_interest
    FROM mbmembmaster
    WHERE coop_id = $P{as_coopid}
      AND resign_status = 0
      AND membgroup_code BETWEEN $P{as_smembgroup} AND $P{as_emembgroup}
    GROUP BY coop_id, membgroup_code
) g
LEFT JOIN mbucfmembgroup mp ON mp.membgroup_code = g.membgroup_code
LEFT JOIN (
    SELECT
           mbs.coop_id,
           mbs.membgroup_code,
           SUM(sm.sharestk_amt * NVL(st.unitshare_value,0)) AS share_balance
    FROM shsharemaster sm
    JOIN mbmembmaster mbs
      ON mbs.coop_id   = sm.coop_id
     AND mbs.member_no = sm.member_no
     AND mbs.resign_status = 0
    LEFT JOIN shsharetype st
      ON st.coop_id = sm.coop_id
     AND st.sharetype_code = sm.sharetype_code
    WHERE sm.sharestk_amt > 0
    GROUP BY mbs.coop_id, mbs.membgroup_code
) sa ON sa.coop_id = g.coop_id AND sa.membgroup_code = g.membgroup_code
LEFT JOIN (
    SELECT
           mbn.coop_id,
           mbn.membgroup_code,
           SUM(lnm.principal_balance) AS principal_balance,
           SUM(lnm.loanapprove_amt)   AS loanapprove_amt,
           SUM(lnm.interest_arrear)   AS interest_arrear
    FROM lncontmaster lnm
    JOIN mbmembmaster mbn
      ON mbn.coop_id   = lnm.coop_id
     AND mbn.member_no = lnm.member_no
     AND mbn.resign_status = 0
    WHERE lnm.principal_balance > 0 and lnm.contract_status > 0
    GROUP BY mbn.coop_id, mbn.membgroup_code
) la ON la.coop_id = g.coop_id AND la.membgroup_code = g.membgroup_code
WHERE
NVL(sa.share_balance,0) > 0
ORDER BY g.membgroup_code]]>
	</queryString>
