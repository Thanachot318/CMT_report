	<queryString>
		<![CDATA[SELECT
    DPDEPTMASTER.DEPTACCOUNT_NO,
    DPDEPTMASTER.DEPTTYPE_CODE,
    DPDEPTMASTER.MEMBER_NO,
    DPDEPTMASTER.DEPTOPEN_DATE,
    DPDEPTMASTER.DEPTACCOUNT_NAME,
    DPDEPTMASTER.PRNCBAL,
    DPDEPTMASTER.CLEARINGCHQ_AMT,
    dpucfdeptmaintype.deptmain_desc
FROM
    DPDEPTMASTER
    JOIN dpucfdeptmaintype ON DPDEPTMASTER.DEPTTYPE_CODE = dpucfdeptmaintype.deptmain_type
    JOIN mbucfcategory ON DPDEPTMASTER.MEMBCAT_CODE = mbucfcategory.MEMBCAT_CODE
WHERE
    DPDEPTMASTER.COOP_ID = $P{as_coopid}
    AND DPDEPTMASTER.PRNCBAL BETWEEN $P{as_sprnbal} AND $P{as_eprnbal}
    AND DPDEPTMASTER.DEPTTYPE_CODE BETWEEN $P{as_start_type} AND $P{as_end_type}
    AND DPDEPTMASTER.MEMBCAT_CODE BETWEEN $P{as_smemcat} AND $P{as_ememcat}
    AND DPDEPTMASTER.DEPTCLOSE_STATUS = 0
ORDER BY DPDEPTMASTER.DEPTTYPE_CODE,DPDEPTMASTER.DEPTACCOUNT_NO]]>
	</queryString>
