	<queryString>
		<![CDATA[SELECT
WC.COOP_NAME,
SLP.DEPTACCOUNT_NO,
MT.MEMBER_NO,
trim(PN.PRENAME_DESC)||trim(MT.DEPTACCOUNT_NAME)||'  '||trim(MT.DEPTACCOUNT_SNAME) as Fullname,
TY.WCMEMBERTYPE_DESC,
SLP.DEPTSLIP_DATE,
SLP.DEPTSLIP_AMT,
MY.MONEYTYPE_DESC

FROM
WCDEPTSLIP SLP
LEFT JOIN CMUCFMONEYTYPE MY ON SLP.CASH_TYPE = MY.MONEYTYPE_CODE
LEFT JOIN WCDEPTMASTER MT ON SLP.COOP_ID = MT.COOP_ID AND SLP.WC_ID = MT.WC_ID AND SLP.DEPTACCOUNT_NO = MT.DEPTACCOUNT_NO
LEFT JOIN MBUCFPRENAME PN ON MT.PRENAME_CODE = PN.PRENAME_CODE
LEFT JOIN WCMEMBERTYPE TY ON MT.COOP_ID = TY.COOP_ID AND MT.WC_ID = TY.WC_ID AND MT.WFTYPE_CODE = TY.WFTYPE_CODE
LEFT JOIN WCCONTCOOP WC ON SLP.COOP_ID = WC.COOP_ID AND SLP.WC_ID = WC.WC_ID
WHERE SLP.COOP_ID = $P{as_coopid}
AND SLP.WC_ID = $P{as_wcid}
AND SLP.ITEM_STATUS = 1
and trunc(SLP.DEPTSLIP_DATE) = to_date($P{adtm_deptslip_date}, 'YYYYMMDD')
AND SLP.CASH_TYPE = $P{as_cashtype}
AND SLP.DEPTITEMTYPE_CODE NOT IN ('WFF')
ORDER BY SLP.CASH_TYPE,SLP.DEPTSLIP_DATE]]>
	</queryString>
