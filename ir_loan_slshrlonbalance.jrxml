	<queryString>
		<![CDATA[select
sl.*,
ft_gatmbname(sl.coop_id, sl.member_no) as fullname,
mp.membgroup_desc
from slshrlonbalance sl
left join mbmembmaster mb on mb.member_no = sl.member_no
left join mbucfmembgroup mp on mb.membgroup_code = mp.membgroup_code
where
sl.coop_id = $P{as_coopid}
and mb.resign_status = 0
and sl.balance_date = $P{adtm_date}
and mb.membgroup_code between $P{as_smembgroup} and $P{as_emembgroup}
order by sl.balance_date, mb.membgroup_code, sl.member_no, sl.seq_no]]>
	</queryString>
