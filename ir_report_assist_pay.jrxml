	<queryString>
		<![CDATA[select ao.member_no
,mb.prename_code
,mb.memb_name
,mb.memb_surname
,ao.slip_date
,ao.assisttype_code,
(select ut.assisttype_desc from assucfassisttype ut where ut.assisttype_code=ao.assisttype_code) assisttype_desc,
(select ut.assisttype_group from assucfassisttype ut where ut.assisttype_code=ao.assisttype_code) assisttype_group,
(select mp.prename_desc from mbucfprename mp where mb.prename_code=mp.prename_code)prename_desc,
(select cc.coop_name from cmcoopconstant cc where mb.coop_id=cc.coop_id) coop_name
,sum(aod.itempay_amt) as  payout_amt
,aod.moneytype_code
,aod.expense_bank
,aod.expense_accid
,aod.deptaccount_no
,CMUCFBANK.bank_shortname_t
,dpucfdepttype.depttype_desc
,aod.methpaytype_code
,aod.description
from assslippayout ao
join assslippayoutdet aod on ao.assistslip_no = aod.assistslip_no
join mbmembmaster mb on ao.member_no=mb.member_no and ao.coop_id = mb.coop_id
left join CMUCFBANK on aod.expense_bank=CMUCFBANK.bank_code
--left join cmucfbankbranch on ASSREQMASTERGAIN.expense_branch = cmucfbankbranch.branch_id and ASSREQMASTERGAIN.expense_bank=cmucfbankbranch.bank_code
left join dpdeptmaster on aod.deptaccount_no = dpdeptmaster.deptaccount_no
left join dpucfdepttype on dpdeptmaster.depttype_code =dpucfdepttype.depttype_code and dpucfdepttype.membcat_code = mb.membcat_code
where ao.slip_date between $P{date1} and $P{date2}
and ao.assisttype_code between $P{assisttype_start} and $P{assisttype_end}
and ao.slip_status = 1
and ao.coop_id = $P{as_coopid}
group by ao.member_no
,mb.prename_code
,mb.memb_name
,mb.memb_surname
,ao.slip_date
,ao.assisttype_code
,mb.prename_code
,mb.coop_id
,aod.moneytype_code
,aod.expense_bank
,aod.expense_accid
,aod.deptaccount_no
,CMUCFBANK.bank_shortname_t
,dpucfdepttype.depttype_desc
,aod.methpaytype_code
,aod.description
order by ao.assisttype_code,ao.slip_date]]>
	</queryString>
