	<queryString>
		<![CDATA[SELECT
    mb.member_no,
    ft_getmbname(mb.coop_id, mb.member_no) AS fullname,
    mb.membgroup_code,
    mp.membgroup_desc,
    la.loantype_code AS loantype_code,
    la.loancontract_no AS loancontract_no,
    NVL(sa.share_balance,0) AS share_balance,
    NVL(la.loanapprove_amt,0) AS loanapprove_amt,
    NVL(la.withdrawable_amt,0) AS withdrawable_amt,
    NVL(la.principal_balance,0) AS principal_balance,
    NVL(la.period_payamt,0) AS period_payamt,
    NVL(la.period_payment,0) AS period_payment
FROM mbmembmaster mb
LEFT JOIN mbucfmembgroup mp ON mb.membgroup_code = mp.membgroup_code
LEFT JOIN (
    SELECT
        sm.coop_id,
        sm.member_no,
        SUM(NVL(sm.sharestk_amt,0) * NVL(st.unitshare_value,0)) AS share_balance
    FROM shsharemaster sm
    JOIN shsharetype st ON st.coop_id = sm.coop_id AND st.sharetype_code = sm.sharetype_code
    WHERE sm.sharestk_amt > 0
    GROUP BY sm.coop_id, sm.member_no
) sa ON sa.coop_id = mb.coop_id AND sa.member_no = mb.member_no
LEFT JOIN (
    SELECT
        lnm.coop_id,
        lnm.member_no,
        lnm.loantype_code,
        lnm.loancontract_no,
        NVL(lnm.loanapprove_amt,0) AS loanapprove_amt,
        NVL(lnm.withdrawable_amt,0) AS withdrawable_amt,
        NVL(lnm.principal_balance,0) AS principal_balance,
        NVL(lnm.period_payamt,0) AS period_payamt,
        NVL(lnm.period_payment,0) AS period_payment
    FROM lncontmaster lnm
    JOIN lnloantype lnt ON lnt.loantype_code = lnm.loantype_code
    WHERE lnm.principal_balance > 0 and lnm.contract_status > 0
) la ON la.coop_id = mb.coop_id AND la.member_no = mb.member_no
WHERE mb.coop_id = $P{as_coopid}
  AND mb.resign_status = 0
  AND mb.membgroup_code BETWEEN $P{as_smembgroup} AND $P{as_emembgroup}
  AND NVL(sa.share_balance,0) > 0
ORDER BY
    mb.membgroup_code,
    mb.member_no,
    la.loantype_code,
    la.loancontract_no]]>
	</queryString>
