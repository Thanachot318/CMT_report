	<parameter name="as_coopid" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[select
$P{as_period} as PERIOD,
cm.coop_taxid AS COOP_TEXID,
concat(mup.prename_desc,hr.emp_name) as EMPNAME,
hr.emp_surname AS EMP_SURNAME,
hr.id_card AS ID_CARD,
(select refslip_date from finreceivetoetc where refsystem_code = 'HR' and refslip_no = concat($P{as_period},'STA_01') and refslipitem_code = 'HRT' and item_amt > 0) as PAYROLL_DATE,
(select hpd.item_amt from hrpayroll hp , hrpayrolldet hpd where hr.emp_no = hp.emp_no and hp.payrollslip_no = hpd.payrollslip_no and hp.payroll_period = $P{as_period} and hpd.salitem_code = 'R01'  and hpd.salitem_code is not null) as R01,
(select hpd.item_amt from hrpayroll hp , hrpayrolldet hpd where hr.emp_no = hp.emp_no and hp.payrollslip_no = hpd.payrollslip_no and hp.payroll_period = $P{as_period} and hpd.salitem_code = 'P01' and hpd.salitem_code is not null) as P01
from
hremployee hr join cmcoopconstant cm on hr.coop_id = cm.coop_id
                      join mbucfprename mup on hr.prename_code = mup.prename_code
                      join hrpayroll hp1 on hr.emp_no = hp1.emp_no
where hr.emptype_code = '01'
and hr.coop_id = $P{as_coopid}
and hp1.payroll_period = $P{as_period} order by hr.emp_no]]>
	</queryString>
