	<parameter name="loanrequest_docno" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT
lnreqloan.LOANREQUEST_DOCNO ,
    NULL AS grp_desc,
    lnreqloan.LOANREQUEST_DATE AS LOANREQUEST_DATE,
    (CASE
        WHEN lnreqloancoll.LOANCOLLTYPE_CODE = '01' THEN lnreqloancoll.REF_COLLNO
        ELSE NULL
    END) AS MEMBER_NO,
    LNUCFLOANCOLLTYPE.LOANCOLLTYPE_DESC  AS coll_desc,
    (CASE
        WHEN lnreqloancoll.LOANCOLLTYPE_CODE = '01' THEN FT_GETMBNAME(lnreqloancoll.coop_id, lnreqloancoll.REF_COLLNO)
        ELSE NULL
    END) AS name,
    NULL AS SALARY_AMOUNT,
    NULL AS INCOMEMONTH_OTHER,
    NULL AS SHARE_BALANCE,
    NULL AS LOANTYPE_CODE,
    NULL AS LOANPERMISS_AMT,
    lnreqloancoll.COLLACTIVE_AMT AS LOANREQUEST_AMT,
    NULL AS loan_clr,
    NULL AS MONEYTYPE_DESC,
    NULL AS RECVESTIMATE_AMT,
    NULL AS ENTRY_ID
FROM
    lnreqloancoll
LEFT JOIN lnreqloan ON lnreqloancoll.loanrequest_docno = lnreqloan.loanrequest_docno
LEFT JOIN  LNUCFLOANCOLLTYPE ON LNUCFLOANCOLLTYPE.LOANCOLLTYPE_CODE = lnreqloancoll.LOANCOLLTYPE_CODE


where lnreqloancoll.LOANREQUEST_DOCNO = $P{loanrequest_docno}]]>
	</queryString>
