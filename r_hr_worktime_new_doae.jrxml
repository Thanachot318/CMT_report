	<queryString>
		<![CDATA[select
distinct
he.number_report,
mp.prename_desc||he.emp_name||' '||he.emp_surname AS full_name,
ht.emp_no,
nvl(hw.work_in,0) as work_in,
ht.work_date,
nvl(hw.work_out,0) as work_out,
hs.pos_desc,
ht.work_time,
ht.back_time,
(case when ht.remark_code is not null then hur.remark_desc
when hl.emp_no is not null    then  hc.leave_desc
when ha.late_cause = '2' then 'สาย'
when  TO_CHAR(ht.end_time , 'hh24') ||'.'|| TO_CHAR(ht.end_time , 'mi') = '00.00' and  TO_CHAR(ht.start_time , 'hh24') ||'.'|| TO_CHAR(ht.start_time , 'mi') = '00.00'  and trim(ht.worktime_code) ='HO'   then 'วันหยุด'
when  TO_CHAR(ht.end_time , 'hh24') ||'.'|| TO_CHAR(ht.end_time , 'mi') != '00.00' and  TO_CHAR(ht.start_time , 'hh24') ||'.'|| TO_CHAR(ht.start_time , 'mi') != '00.00'  and trim(ht.worktime_code) ='HO'   then 'ทำงานในวันหยุด'
when   (ha.late_cause = '1' or ha.late_cause = '0')  and trim(ht.worktime_code) ='HO' and  (NVL(ho.ot_code,0)  = '01'  or ho.ot_code  is null) then 'วันหยุด'
when  TO_CHAR(ht.end_time , 'hh24') ||'.'|| TO_CHAR(ht.end_time , 'mi') = '00.00' and  TO_CHAR(ht.start_time , 'hh24') ||'.'|| TO_CHAR(ht.start_time , 'mi') = '00.00'   then 'ขาด'
when  TO_CHAR(ht.end_time , 'hh24') ||'.'|| TO_CHAR(ht.end_time , 'mi') = '00.00' or TO_CHAR(ht.start_time , 'hh24') ||'.'|| TO_CHAR(ht.start_time , 'mi') = '00.00'   then 'ลงเวลางานไม่ครบ'
when  ht.back_time != '' or  ht.back_time  is not null    then 'กลับก่อนเวลา'
when  TO_CHAR(ht.end_time , 'hh24') ||'.'|| TO_CHAR(ht.end_time , 'mi') != '00.00' and  TO_CHAR(ht.start_time , 'hh24') ||'.'|| TO_CHAR(ht.start_time , 'mi') != '00.00' then 'เข้างานปกติ'
else  '' end) as worktime_code2,
(case when ha.late_cause = '2' and ht.remark_code is null then 'สาย' when (ht.remark_code is not null and ht.remark_code <> '14') then hur.remark_desc else '' end) as worktime_code,
$P{as_sdate} as date1,
$P{as_edate} as date2,
ht.work_date,
(case when hl.leave_code  in ('09','10') then '15.00'
else TO_CHAR( ht.start_time , 'hh24') ||'.'|| TO_CHAR(ht.start_time , 'mi') end ) as start_time,
TO_CHAR( ht.end_time, 'hh24') ||'.'|| TO_CHAR(ht.end_time , 'mi') as end_time,
to_number((case when hl.leave_code  in ('09','10') then '15.00'
else TO_CHAR( ht.start_time , 'hh24') end )) as startnum,
to_number(TO_CHAR( ht.end_time, 'hh24')) as endnum,
'' as remark,
(case when (hl.emp_no is not null or ht.remark_code in ('01','02','03','11','12','13','14')) then '0' else NVL(to_char( ha.late_time, 'HH24:MI' ),'0') end) as  late_time,
to_char((case when (hl.emp_no is not null or ht.remark_code in ('01','02','03','11','12','13','14')) then ''
                     when ht.worktime_code = 'LW' then '1' else '' end)) as abnormal,
nvl((select count(ht1.emp_no) from hrlogworktime ht1  where ht1.emptype_code= $P{as_emptype} and  TO_CHAR(ht1.end_time , 'hh24') ||'.'|| TO_CHAR(ht1.end_time , 'mi') = '00.00' and  TO_CHAR(ht1.start_time , 'hh24') ||'.'|| TO_CHAR(ht1.start_time , 'mi') = '00.00'  and   trunc(ht1.work_date) = trunc(ht.work_date)),0) as lack,
nvl((select count(ht2.emp_no) from hrlogworktime ht2  where  ht2.emptype_code = $P{as_emptype}  and (TO_CHAR(ht2.end_time , 'hh24') ||'.'|| TO_CHAR(ht2.end_time , 'mi') != '00.00' or  TO_CHAR(ht2.start_time , 'hh24') ||'.'|| TO_CHAR(ht2.start_time , 'mi') != '00.00')  and trunc(ht2.work_date) = trunc(ht.work_date)),0) as normal,
(select mb.prename_desc||hr.emp_name||' '||hr.emp_surname
from hremployee hr  left join mbucfprename mb on mb.prename_code = hr.prename_code where hr.emp_status = 1
and  hr.pos_code = '4' ) as fullname_leader,
(select mb.prename_desc||hr.emp_name||' '||hr.emp_surname
from hremployee hr  left join mbucfprename mb on mb.prename_code = hr.prename_code where hr.emp_status = 1
and hr.deptgrp_code = '02' and hr.pos_code = '3' ) as fullname_lowmg1,
(select mb.prename_desc||hr.emp_name||' '||hr.emp_surname
from hremployee hr  left join mbucfprename mb on mb.prename_code = hr.prename_code where hr.emp_status = 1
and hr.deptgrp_code = '01'  and hr.pos_code = '1'  ) as fullname_mg
from hrlogworktime ht inner join hremployee he on ht.emp_no = he.emp_no
left join hrlogleave hl on ht.emp_no = hl.emp_no  and hl.apv_status =1
and ht.work_date between  trunc(hl.leave_from) and trunc(hl.leave_to)
 left join  hrucfleavecode hc on hl.leave_code = hc.leave_code
left join hrloglate ha on ht.emp_no = ha.emp_no  and ht.work_date = ha.late_date
left join mbucfprename mp on he.prename_code = mp.prename_code
left join hrucfworktimecode hw on he.worktime_code = hw.worktime_code
left join hrucfpositiondet hd  on he.pos_code = hd.pos_code
left join hrucfpositionmas hs on hd.pos_mas = hs.pos_mas
left join hrw_ot ho on  ht.emp_no = ho.emp_no  and ho.apv_ot_status = 1 and trunc(ho.date_work) = ht.work_date
left join hrucfworktimeremark hur on ht.remark_code = hur.remark_code
where he.emp_status = 1
and he.coop_id= $P{as_coopid} and ht.emp_no between $P{as_sempno} and $P{as_eempno}
and  trunc(ht.work_date) between $P{as_sdate} and $P{as_edate}
and ht.emptype_code = $P{as_emptype}
order by  to_number(he.number_report),ht.work_date,ht.emp_no]]>
	</queryString>
