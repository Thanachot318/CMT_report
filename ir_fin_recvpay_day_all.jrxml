	<queryString>
		<![CDATA[select
f.slip_no,
f.payment_desc,
f.member_no,
f.nonmember_detail,
d.item_amt,
f.itemnet_amt,
f.entry_id,
f.entry_date,
d.cash_type,
f.payment_status,
f.pay_recv_status,
c.coop_name,
f.remark,
d.account_id,
d.tax_amt,
d.vat_amt,
d.slipitem_desc
from
finslip f
left join finslipdet d on f.slip_no = d.slip_no
left join cmcoopconstant c on f.coop_id = c.coop_id
where f.slip_date = $P{adtm_date} and f.coop_id = $P{as_coopid}
and f.payment_status = 1
group by
f.slip_no,
f.payment_desc,
f.member_no,
f.nonmember_detail,
d.item_amt,
f.itemnet_amt,
f.entry_id,
f.entry_date,
d.cash_type,
f.payment_status,
f.pay_recv_status,
c.coop_name,
f.remark,
d.account_id,
d.tax_amt,
d.vat_amt,
d.slipitem_desc
order by f.pay_recv_status,f.slip_no]]>
	</queryString>
