	<queryString>
		<![CDATA[select
lnl.*,
lnm.*,
(select province_desc from mbucfprovince where province_code = lnl.pos_province)  as province_desc,
(select district_desc from mbucfdistrict where district_code = lnl.pos_amphur)  as amphur_desc,
(select tambol_desc from mbucftambol where tambol_code = lnl.pos_tambol) as tambol_desc,
ft_getmbname(lnm.coop_id, lnm.member_no) as mem_name,
ft_readtbaht(lnm.est_price) as m_bath,
(select tambol_desc from mbucftambol where tambol_code = lnm.mrtg_tambol1) as tambol_desc2,
(select district_desc from mbucfdistrict where district_code = lnm.mrtg_amphur1)  as amphur_desc2,
(select province_desc from mbucfprovince where province_code = lnm.mrtg_province1)  as province_desc2,
(select tambol_desc from mbucftambol where tambol_code = lnm.autzd_tambol) as tambol_desc3,
(select district_desc from mbucfdistrict where district_code = lnm.autzd_amphur)  as amphur_desc3,
(select province_desc from mbucfprovince where province_code = lnm.autzd_province)  as province_desc3,
(select coop_name from cmcoopmaster where coop_id = lnm.coop_id) as coop_name,
(select addr_moo from cmcoopmaster where coop_id = lnm.coop_id) as addr_moo
from lncollmaster lnm
join lncollmastland lnl on lnm.collmast_no = lnl.collmast_no
where lnm.collmast_no = $P{colmast_no}
and lnl.coop_id = $P{coopid}]]>
	</queryString>
