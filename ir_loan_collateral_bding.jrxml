	<parameter name="collmast_no" class="java.lang.String"/>
	<queryString>
		<![CDATA[select
lnb.*,
lnt.buildingtype_desc,
(select province_desc from mbucfprovince where province_code = lnb.bd_province)  as province_desc,
(select district_desc from mbucfdistrict where district_code = lnb.bd_amphur)  as amphur_desc,
(select tambol_desc from mbucftambol where tambol_code = lnb.bd_tambol) as tambol_desc
from lncollmastbuilding lnb
left join lnucfbuildingtype lnt on lnb.buildingtype_code = lnt.buildingtype_code
where lnb.collmast_no = $P{collmast_no}
order by lnb.buildingtype_code]]>
	</queryString>
