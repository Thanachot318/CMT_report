	<queryString>
		<![CDATA[select
cnt.coop_name
, cnt.coop_shortname
, pre.prename_desc || ' ' || trim(mt.deptaccount_name) || ' ' || trim(mt.deptaccount_sname) as Full_name
, mt.deptaccount_no
, to_char( mt.deptopen_date,'dd/mm/')||to_char(to_number(to_char( mt.deptopen_date,'yyyy'))+543) as deptopen_date
, mt.deptopen_date as test
 from wcdeptmaster mt
join wccontcoop cnt on mt.wc_id = cnt.wc_id and mt.coop_id = cnt.coop_id
left join mbucfprename pre on trim(to_char(mt.prename_code,'000')) = pre.prename_code
where mt.wc_id = $P{as_wcid}
and mt.deptaccount_no between $P{as_sdept} and $P{as_edept}]]>
	</queryString>
