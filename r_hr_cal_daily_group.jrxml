	<parameter name="as_period" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="as_coopid" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="emptype_code" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[select
substr($P{as_period},0,4) as year,
case     substr($P{as_period},5,2)    when '01' then 'มกราคม'
                            when '02' then 'กุมภาพันธ์'
                            when '03' then 'มีนาคม'
                            when '04' then 'เมษายน'
                    when '05' then 'พฤษภาคม'
                            when '06' then 'มิถุนายน'
                            when '07' then 'กรกฎาคม'
                            when '08' then 'สิงหาคม'
                            when '09' then 'กันยายน'
                            when '10' then 'ตุลาคม'
                            when '11' then 'พฤศจิกายน'
                            when '12' then 'ธันวาคม'
                            else ''
                            end as month,
(case when $P{emptype_code} = '01' then 'ลูกจ้างประจำ'
when $P{emptype_code} = '02' then 'ลูกชั่วคราวรายเดือน'
when $P{emptype_code} = '03' then 'ลูกชั่วคราว' else'' end) as emptype,
nvl((select special_day from hrworkcalendar where  year = substr($P{as_period},0,4) and month = substr($P{as_period},5,2)),0) as special_day,
(case when hr.emptype_code = '03' then (select holiday from hrworkcalendar where  year = substr($P{as_period},0,4) and month = substr($P{as_period},5,2))
else (select holiday_full from hrworkcalendar where  year = substr($P{as_period},0,4) and month = substr($P{as_period},5,2)) end) as holiday,
nvl((select daysinmonth from hrworkcalendar where  year = substr($P{as_period},0,4) and month = substr($P{as_period},5,2)),0) as workdays,
nvl((case when hr.emptype_code = '03' then (select work_day from hrworkcalendar where  year = substr($P{as_period},0,4) and month = substr($P{as_period},5,2))
else (select work_day_full from hrworkcalendar where  year = substr($P{as_period},0,4) and month = substr($P{as_period},5,2)) end),0) as work_day,
nvl((select sum(hd.item_amt) from hrpayrolldet hd where hd.salitem_code = 'R01' and substr(hd.payrollslip_no,0,6) = $P{as_period} and substr(hd.payrollslip_no,7,1) = replace(hr.emptype_code,'0','')),0) as salary,
nvl((select sum(hd.item_amt) from hrpayrolldet hd where hd.salitem_code = 'R02' and substr(hd.payrollslip_no,0,6) = $P{as_period} and substr(hd.payrollslip_no,7,1) = replace(hr.emptype_code,'0','')),0) as ot_amt,
nvl((select sum(hd.item_amt) from hrpayrolldet hd where hd.salitem_code = 'P02' and substr(hd.payrollslip_no,0,6) = $P{as_period} and substr(hd.payrollslip_no,7,1) = replace(hr.emptype_code,'0','')),0) as P02,
nvl((select sum(hd.item_amt) from hrpayrolldet hd where hd.salitem_code = 'P01' and substr(hd.payrollslip_no,0,6) = $P{as_period} and substr(hd.payrollslip_no,7,1) = replace(hr.emptype_code,'0','')),0) as P01,

nvl((select sum(hd.item_amt) from hrpayrolldet hd where hd.salitem_code = 'R01' and substr(hd.payrollslip_no,0,6) = $P{as_period} and substr(hd.payrollslip_no,7,1) = replace(hr.emptype_code,'0','')),0) +
nvl((select sum(hd.item_amt) from hrpayrolldet hd where hd.salitem_code = 'R02' and substr(hd.payrollslip_no,0,6) = $P{as_period} and substr(hd.payrollslip_no,7,1) = replace(hr.emptype_code,'0','')),0) -
nvl((select sum(hd.item_amt) from hrpayrolldet hd where hd.salitem_code = 'P02' and substr(hd.payrollslip_no,0,6) = $P{as_period} and substr(hd.payrollslip_no,7,1) = replace(hr.emptype_code,'0','')),0) -
nvl((select sum(hd.item_amt) from hrpayrolldet hd where hd.salitem_code = 'P01' and substr(hd.payrollslip_no,0,6) = $P{as_period} and substr(hd.payrollslip_no,7,1) = replace(hr.emptype_code,'0','')),0) as total

from hremployee hr
where hr.coop_id = $P{as_coopid} and hr.emptype_code = $P{emptype_code} and rownum = 1
order by hr.emp_no]]>
	</queryString>
