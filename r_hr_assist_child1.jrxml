	<queryString>
		<![CDATA[select  mup.prename_desc || hr.emp_name || ' ' || hr.emp_surname as fullname,
h.emp_no ,
h.coop_id,
$P{account_year} as account_year,
$P{as_idcard} as idcard,
h.assist_docno,
hup.pos_desc,
hud.deptgrp_desc,
h.assist_name,
h.assist_posit,
hf.birth_date,
hc.education_desc,
h.assist_amt,
ft_readtbaht(h.assist_amt) as thaibaht,
h.pay_type,
h.assist_amp
from
hremployee hr ,
hremployeeassist h ,
mbucfprename mup ,
 hrucfpositiondet hup ,
 hrucfpositionmas hum ,
 hrucfdeptgrp hud  ,
hrucfeducation hc,
hremployeefamily hf
where hr.emp_no = h.emp_no
and hr.prename_code = mup.prename_code
and  hr.pos_code = hup.pos_code(+)
and  hup.pos_mas = hum.pos_mas(+)
and  hr.deptgrp_code = hud.deptgrp_code(+)
and h.education_code = hc.education_code(+)
and hf.f_name = h.assist_name(+)
and hf.emp_no = h.emp_no(+)
and  h.assist_code = '02'
and h.assist_docno = $P{assist_docno}
 and h.coop_id =$P{as_coopid}]]>
	</queryString>
