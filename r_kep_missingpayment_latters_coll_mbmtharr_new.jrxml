	<queryString>
		<![CDATA[select ft_getmbname(ma.coop_id,ma.member_no) as fullname, ma.member_no,
cc.coop_name, cc.manager, ma.recv_period

, ft_readtbaht( ma.principal_arrear+ma.interest_arrear ) as readitemarr_amt,ma.principal_arrear+ma.interest_arrear as itemarr_amt
,substr(ma.recv_period, 1 , 4 ) as year
,    (case  when  trim(substr(ma.recv_period,5,6))= '01'  then 'มกราคม'
    when  trim(substr(ma.recv_period,5,6))= '02' then 'กุมภาพันธ์'
    when  trim(substr(ma.recv_period,5,6))= '03' then 'มีนาคม'
    when  trim(substr(ma.recv_period,5,6))= '04' then 'เมษายน'
    when  trim(substr(ma.recv_period,5,6))= '05' then 'พฤษภาคม'
    when  trim(substr(ma.recv_period,5,6))= '06' then 'มิถุนายน'
    when  trim(substr(ma.recv_period,5,6))= '07' then 'กรกฎาคม'
    when  trim(substr(ma.recv_period,5,6))= '08' then 'สิงหาคม'
    when  trim(substr(ma.recv_period,5,6))= '09' then 'กันยายน'
    when  trim(substr(ma.recv_period,5,6))= '10' then 'ตุลาคม'
    when  trim(substr(ma.recv_period,5,6))= '11' then 'พฤศจิกายน'
    when  trim(substr(ma.recv_period,5,6))= '12' then 'ธันวาคม'
    else '-'  END) as month

, lc.ref_collno, lc.description, ma.loancontract_no, lm.lastpayment_date, lm.lastprocess_date, lm.lastcalint_date, lm.startcont_date,lm.loanapprove_amt, lm.period_payment
from mbmembmtharrear ma
join mbmembmaster mb on mb.member_no = ma.member_no
join cmcoopconstant cc on ma.coop_id = cc.coop_id
join lncontmaster lm on ma.loancontract_no = lm.loancontract_no and ma.member_no = lm.member_no
join lncontcoll lc on lm.loancontract_no = lc.loancontract_no
where ma.coop_id = $P{coop_id}
and ma.recv_period =  $P{recv_period}
and  ma.member_no between  $P{smember_no}  and  $P{emember_no}
and mb.membgroup_code between $P{smembgroup_code} and $P{emembgroup_code}
order by ma.member_no asc]]>
	</queryString>
