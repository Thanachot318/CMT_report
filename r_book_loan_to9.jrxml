	<queryString>
		<![CDATA[select
lnl.*,
lnm.*,
cm.coop_name,
cm.coop_road as addr_road,
substr(cm.coop_addr, instr(cm.coop_addr, ' ')+1, length(cm.coop_addr)) as addr_no,
(select tambol_desc from mbucftambol where tambol_code = cm.tambol_code) as ADDR_TAMBOL,
(select district_desc from mbucfdistrict where district_code = cm.district_code)  as ADDR_AMPHUR,
(select province_desc from mbucfprovince where province_code = cm.province_code)  as ADDR_PROVINCE,

ft_getmbname(lnm.coop_id, lnm.member_no) as mem_name,
ft_readtbaht(lnm.est_price) as ESTPRICE_BAHT,

(select tambol_desc from mbucftambol where tambol_code = lnm.mrtg_tambol1) as mrtg_tamboldesc1,
(select district_desc from mbucfdistrict where district_code = lnm.mrtg_amphur1)  as mrtg_amphurdesc1,
(select province_desc from mbucfprovince where province_code = lnm.mrtg_province1)  as mrtg_provincedesc1,


(select tambol_desc from mbucftambol where tambol_code = lnl.pos_tambol) as tambol_desc,
(select district_desc from mbucfdistrict where district_code = lnl.pos_amphur)  as district_desc,
(select province_desc from mbucfprovince where province_code = lnl.pos_province)  as province_desc

from lncollmaster lnm
join lncollmastland lnl on lnm.collmast_no = lnl.collmast_no
left join cmcoopconstant cm on lnm.coop_id = cm.coop_Id
where lnm.collmast_no = $P{colmast_no}
and lnl.coop_id = $P{coopid}]]>
	</queryString>
