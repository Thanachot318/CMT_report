	<queryString>
		<![CDATA[select
f.slip_no,
f.payment_desc,
f.member_no,
f.nonmember_detail,
d.itemnet_amt,
f.itemnet_amt as sum_amt,
f.entry_id,
f.entry_date,
d.cash_type,
f.payment_status,
f.pay_recv_status
from
finslip f
left join finslipdet d on f.slip_no = d.slip_no
where f.entry_date = $P{as_date} and f.entry_id = $P{as_user}
group by
f.slip_no,
f.payment_desc,
f.member_no,
f.nonmember_detail,
d.itemnet_amt,
f.itemnet_amt,
f.entry_id,
f.entry_date,
d.cash_type,
f.payment_status,
f.pay_recv_status
order by f.pay_recv_status,f.slip_no]]>
	</queryString>
