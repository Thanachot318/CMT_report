	<queryString>
		<![CDATA[select mb.member_no , ft_getmbname( mb.coop_id , mb.member_no ) as fullname,
mg.membgroup_code, mg.membgroup_desc , mb.salary_amount , mb.pausekeep_flag,mb.pausekeep_date,
sh.sharestk_amt * 10 as sharestk_value,cm.coop_name,sh.payment_status
from mbmembmaster mb
left join mbucfmembgroup mg on mb.membgroup_code = mg.membgroup_code
left join shsharemaster sh on mb.member_no = sh.member_no
left join cmcoopconstant cm on mb.coop_id = cm.coop_id
where mb.member_status = 1 and sh.payment_status < 1 and sharestk_amt > 0  and mb.coop_id =  $P{as_coopid} and mb.membgroup_code between  $P{as_smemno} and  $P{as_ememno}

and mb.member_no not in ( select member_no from lncontmaster where principal_balance > 0 and payment_status > -13)

order by membgroup_code , mb.member_no]]>
	</queryString>
