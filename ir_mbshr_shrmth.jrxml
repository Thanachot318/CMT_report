	<queryString>
		<![CDATA[select
slclsmthbalance.member_no,
ft_getmbname(slclsmthbalance.coop_id,slclsmthbalance.member_no) as fullname,
mbmembmaster.membtype_code,
mbucfmembtype.membtype_desc,
slclsmthbalance.periodpay_value,
slclsmthbalance.balance_value,
mbmembmaster.membgroup_code,
mbucfmembgroup.membgroup_desc,
mbucfprovince.province_desc
from slclsmthbalance
left join mbmembmaster on slclsmthbalance.member_no = mbmembmaster.member_no
left join mbucfmembtype on mbmembmaster.membtype_code = mbucfmembtype.membtype_code
left join mbucfmembgroup on mbmembmaster.membgroup_code = mbucfmembgroup.membgroup_code
left join mbucfprovince on mbucfmembgroup.province_code = mbucfprovince.province_code
 where slclsmthbalance.clsmth_period = $P{as_recvperiod} and slclsmthbalance.bizz_system = 'SHR'
 and mbmembmaster.membtype_code BETWEEN $P{as_smembtype} AND $P{as_emembtype}
 and mbmembmaster.RESIGN_STATUS != 1
 order by mbmembmaster.membtype_code,mbmembmaster.membgroup_code,slclsmthbalance.member_no]]>
	</queryString>
