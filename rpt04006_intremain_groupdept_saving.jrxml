	<queryString>
		<![CDATA[SELECT dpdeptintremain.operate_date,
         dpdeptintremain.deptaccount_no,
         dpdeptintremain.seq_no,
         dpdeptintremain.calint_to,
         dpdeptintremain.calint_from,
         dpdeptintremain.prncbal,
         dpdeptintremain.int_amt,
         dpdeptintremain.member_no,
         dpdeptintremain.old_intremain,
         dpdeptintremain.prnc_no,
         dpdeptintremain.coop_id,
         dpdeptmaster.deptaccount_name,
         dpdeptmaster.member_no,
         cmcoopconstant.coop_name,
         mbucfcategory.membcat_desc,
         dpucfdepttype.depttype_desc,
         dpucfdepttype.depttype_code,
         dpucfdepttype.membcat_code ,
		'                              ' as report_id
    FROM dpdeptintremain,
         dpdeptmaster,
         cmcoopconstant,
         mbucfcategory,
         dpucfdepttype
   WHERE ( dpdeptintremain.coop_id = dpdeptmaster.coop_id ) and
         ( dpdeptintremain.deptaccount_no = dpdeptmaster.deptaccount_no ) and
         ( cmcoopconstant.coop_id = dpdeptmaster.coop_id ) and
         ( dpdeptmaster.coop_id = dpucfdepttype.coop_id ) and
         ( dpdeptmaster.membcat_code = dpucfdepttype.membcat_code ) and
         ( dpdeptmaster.depttype_code = dpucfdepttype.depttype_code ) and
         ( mbucfcategory.coop_id = dpucfdepttype.coop_id )   and
         ( dpucfdepttype.membcat_code = mbucfcategory.membcat_code ) and
        ( ( DPDEPTINTREMAIN.OPERATE_DATE = $P{adtm_date} ) AND
         ( DPDEPTINTREMAIN.COOP_ID = $P{as_coopid}  ) )]]>
	</queryString>
