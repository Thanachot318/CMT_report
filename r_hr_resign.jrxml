	<queryString>
		<![CDATA[select hr.emp_no,
mup.prename_desc || hr.emp_name || ' ' || hr.emp_surname as full_name,
TO_CHAR(hr.work_date,'dd/mm/yyyy','NLS_CALENDAR=''THAI BUDDHA'' NLS_DATE_LANGUAGE=THAI') as work_date,
TO_CHAR(hr.term_date,'dd/mm/yyyy','NLS_CALENDAR=''THAI BUDDHA'' NLS_DATE_LANGUAGE=THAI') as term_date,
hrr.insure_money,
hrr.resign_money,
hur.resign_desc,
 hr.emp_status
from hremployee hr, mbucfprename mup, hrresign hrr , hrucfresign hur
where hr.prename_code = mup.prename_code
and hr.emp_no between $P{as_sempno} and $P{as_eempno}
and hrr.resign_type = hur.resign_type(+)
and hr.emp_no = hrr.emp_no(+)
and hr.emp_status = 0
and hr.term_date between to_date ($P{date1},'dd/MM/yyyy') and to_date ($P{date2},'dd/MM/yyyy') order by hr.emp_no]]>
	</queryString>
