	<property name="ireport.x" value="0"/>
	<property name="ireport.y" value="0"/>
	<parameter name="as_wcid" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="as_accountno" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[select
stm.deptaccount_no
, mbu.prename_desc
, mt.deptaccount_name
, mt.deptaccount_sname
, wc.coop_name as Main_name
, cm.coop_name as Sub_name
, stm.coop_control
, wt.wcmembertype_desc
, wc.wc_id
, dt.transferee_name
, mt.contact_address
, mbuc.district_desc as district
, pr.province_code as province
, mt.mobiletel
, mt.manage_name
, case  To_char  (stm.operate_date)  when '01' then 'มกราคม'
                            when '02' then 'กุมภาพันธ์'
                            when '03' then 'มีนาคม'
                            when '04' then 'เมษายน'
                            when '05' then 'พฤษภาคม'
                            when '06' then 'มิถุนายน'
                            when '07' then 'กรกฎาคม'
                            when '08' then 'สิงหาคม'
                            when '09' then 'กันยายน'
                            when '10' then 'ตุลาคม'
                            when '11' then 'พฤศจิกายน'
                            when '12' then 'ธันวาคม'
                            else ''
                            end as month_start



, to_char(mt.deptopen_date,'dd/mm/')||to_char(to_number(to_char(mt.deptopen_date,'yyyy'))+543) as deptopen_date
, to_char(stm.operate_date,'dd/mm/')||to_char(to_number(to_char(stm.operate_date,'yyyy'))+543) as operate_date
, sum(case when stm.deptitemtype_code = 'FEE' then stm.deptitem_amt else 0 end) as fee_amt
, sum(case when stm.deptitemtype_code = 'WFY' then stm.deptitem_amt else 0 end) as wfy_amt
, sum(case when stm.deptitemtype_code = 'WPF' then stm.deptitem_amt else 0 end) as wpf_amt
, sum(case when stm.deptitemtype_code = 'WFC' then stm.deptitem_amt else 0 end) as wfc_amt
, sum(stm.prncbal) as stm_prncbal
 from wcdeptstatement stm
join wcdeptmaster mt on ( mt.deptaccount_no = stm.deptaccount_no and mt.wc_id = stm.wc_id )
join wccontcoop wc on (wc.wc_id = stm.wc_id)
join cmcoopmaster cm on (cm.coop_control = stm.coop_control)
join wcmembertype wt on (wt.wftype_code = mt.wftype_code)
join wccodeposit dt  on (dt.deptaccount_no = stm.deptaccount_no and dt.seq_no = 1)
join mbucfdistrict mbuc on (mbuc.district_code = mt.district_code)
left join mbucfprovince pr on (pr.province_code =mt.province_code )
left join mbucfprename mbu on (mbu.prename_code  =  mt.prename_code)
where stm.deptaccount_no = $P{as_accountno}
and stm.wc_id = $P{as_wcid}
group by stm.deptaccount_no, stm.operate_date, mt.deptaccount_name, mt.deptaccount_sname, mbu.prename_desc, cm.coop_name, stm.coop_control,wt.wcmembertype_desc,wc.coop_name ,wc.wc_id
,dt.transferee_name, mt.district_code, mt.province_code
, mt.mobiletel
, mt.manage_name
,mt.contact_address
, mbuc.district_desc
, pr.province_code
,mt.deptopen_date

order by stm.deptaccount_no, stm.operate_date]]>
	</queryString>
