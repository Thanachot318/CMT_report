	<parameter name="as_coopid" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="as_recvperiod" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="as_smembgroup" class="java.lang.String"/>
	<parameter name="as_emembgroup" class="java.lang.String"/>
	<queryString>
		<![CDATA[select
cm.coop_name,
mb.membgroup_code || ' ' || mb.membgroup_desc as membgroup_desc,

SUBSTR(sl.clsmth_period, 1, 4) as year,
(case
	when SUBSTR(sl.clsmth_period, 5, 6) = '01' then 'มกราคม'
	when SUBSTR(sl.clsmth_period, 5, 6) = '02' then 'กุมภาพันธ์'
	when SUBSTR(sl.clsmth_period, 5, 6) = '03' then 'มีนาคม'
	when SUBSTR(sl.clsmth_period, 5, 6) = '04' then 'เมษายน'
	when SUBSTR(sl.clsmth_period, 5, 6) = '05' then 'พฤษภาคม'
	when SUBSTR(sl.clsmth_period, 5, 6) = '06' then 'มิถุนายน'
	when SUBSTR(sl.clsmth_period, 5, 6) = '07' then 'กรกฎาคม'
	when SUBSTR(sl.clsmth_period, 5, 6) = '08' then 'สิงหาคม'
	when SUBSTR(sl.clsmth_period, 5, 6) = '09' then 'กันยายน'
	when SUBSTR(sl.clsmth_period, 5, 6) = '10' then 'ตุลาคม'
	when SUBSTR(sl.clsmth_period, 5, 6) = '11' then 'พฤศจิกายน'
	when SUBSTR(sl.clsmth_period, 5, 6) = '12' then 'ธันวาคม'
	else '' end) as mount_th,

nvl((select count(DISTINCT sl.member_no) as count_1
from slclsmthbalance sl
where  sl.membgroup_code = mb.membgroup_code
and sl.clsmth_period = $P{as_recvperiod} and sl.current_coopid is null and sl.bizztype_status = 1), 0) as count_1,

nvl((select count(DISTINCT sl.member_no) as count
from slclsmthbalance sl
where  sl.membgroup_code = mb.membgroup_code
and sl.bizz_system = 'LON'
and sl.clsmth_period = $P{as_recvperiod}), 0) as count,

nvl((select SUM(sl.balance_value) as balance_value
from slclsmthbalance sl
where  sl.membgroup_code = mb.membgroup_code
and sl.bizz_system = 'SHR'
and sl.bizztype_status != -1
and sl.clsmth_period = $P{as_recvperiod}), 0) as balance_value,

nvl((select SUM(sl.balance_value) as balance_01
from slclsmthbalance sl
join lnloantype ln on sl.bizztype_code = ln.loantype_code
where   sl.membgroup_code = mb.membgroup_code
and ln.loangroup_code  = 01
and sl.clsmth_period = $P{as_recvperiod}), 0) as balance_01,

nvl((select SUM(sl.balance_value) as balance_02
from slclsmthbalance sl
join lnloantype ln on sl.bizztype_code = ln.loantype_code
where   sl.membgroup_code = mb.membgroup_code
and ln.loangroup_code  = 02
and sl.clsmth_period = $P{as_recvperiod}), 0) as balance_02,

nvl((select SUM(sl.balance_value) as balance_03
from slclsmthbalance sl
join lnloantype ln on sl.bizztype_code = ln.loantype_code
where   sl.membgroup_code = mb.membgroup_code
and ln.loangroup_code  = 03
and sl.clsmth_period = $P{as_recvperiod}), 0) as balance_03,

nvl((select sum(lm.interest_payment) as interest_payment
from slclsmthbalance ln
join lncontstatement lm on ln.loancontract_no = lm.loancontract_no
where ln.clsmth_period = $P{as_recvperiod}
and ln.membgroup_code = mb.membgroup_code
and lm.item_status = 1
and to_char(to_number(to_char(lm.slip_date,'yyyymm')) + 54300) between to_char(to_number(substr($P{as_recvperiod},0,4)) - 1) || '10' and $P{as_recvperiod}),0) as interest_payment

from slclsmthbalance sl , mbucfmembgroup mb, lnloantype ln, cmcoopmaster cm
where sl.bizztype_code = ln.loantype_code(+)
and sl.membgroup_code = mb.membgroup_code
and sl.coop_id = cm.coop_id
and sl.clsmth_period = $P{as_recvperiod}
and sl.coop_id = $P{as_coopid}
and sl.membgroup_code between $P{as_smembgroup} and $P{as_emembgroup}
group by cm.coop_name, mb.membgroup_code,  mb.membgroup_desc ,  sl.clsmth_period
order by mb.membgroup_code]]>
	</queryString>
