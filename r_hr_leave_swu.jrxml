	<queryString>
		<![CDATA[SELECT
     DISTINCT hr.emp_no AS emp_no ,
     mup.prename_desc||hr.emp_name||' '||hr.emp_surname AS full_name,
$P{date1} as date1,
$P{date2} as date2,
	(select sum(hrl1.count_status)
	from HRLOGLEAVE hrl1
	where hrl1.leave_date BETWEEN $P{date1} and $P{date2}
	and hrl1.apv_status = '1'
	and hrl1.leave_code = '001' and hrl1.emp_no = hrl.emp_no) as totalday_001,
    (select sum(hrl2.count_status)
	from HRLOGLEAVE hrl2
	where hrl2.leave_date BETWEEN $P{date1} and $P{date2}
	and hrl2.apv_status = '1'
	and hrl2.leave_code = '002' and hrl2.emp_no = hrl.emp_no) as totalday_002,
(select sum(hrl3.count_status)
	from HRLOGLEAVE hrl3
	where hrl3.leave_date BETWEEN $P{date1} and $P{date2}
	and hrl3.apv_status = '1'
	and hrl3.leave_code = '003' and hrl3.emp_no = hrl.emp_no) as totalday_003,
(select sum(hrl4.count_status)
	from HRLOGLEAVE hrl4
	where hrl4.leave_date BETWEEN $P{date1} and $P{date2}
	and hrl4.apv_status = '1'
	and hrl4.leave_code = '004' and hrl4.emp_no = hrl.emp_no) as totalday_004,
(select sum(hrl6.count_status)
	from HRLOGLEAVE hrl6
	where hrl6.leave_date BETWEEN $P{date1} and $P{date2}
	and hrl6.apv_status = '1'
	and hrl6.leave_code = '006' and hrl6.emp_no = hrl.emp_no) as totalday_006,
(select sum(hrl5.count_status)
	from HRLOGLEAVE hrl5
	where hrl5.leave_date BETWEEN $P{date1} and $P{date2}
	and hrl5.apv_status = '1'
	and hrl5.leave_code in ('005','007') and hrl5.emp_no = hrl.emp_no) as totalday_005,

(select sum(count(hrlogworktime.emp_no)) as c_emp_no
from hrlogworktime ,  hrlogworktime_full
where hrlogworktime.work_date = hrlogworktime_full.work_full and hrlogworktime_full.day_desc = 'W' and
(hrlogworktime.start_time is null or hrlogworktime.end_time is null) and hrlogworktime.emp_no = hr.emp_no and hrlogworktime.emp_no = hrlogworktime_full.emp_no and hrlogworktime.work_date BETWEEN $P{date1} and $P{date2} and hrlogworktime.worktime_code <> 'LEA' group by hrlogworktime.emp_no) as not_work,

NVL((select sum(count(hrlogworktime.emp_no)) as c_emp_no
from hrlogworktime
where (hrlogworktime.start_time is null or hrlogworktime.end_time is null) and hrlogworktime.emp_no = hr.emp_no and hrlogworktime.work_date BETWEEN $P{date1} and $P{date2} and hrlogworktime.worktime_code <> 'LEA' group by hrlogworktime.emp_no),0) +

NVL((select sum(hrl1.count_status)
	from HRLOGLEAVE hrl1
	where hrl1.leave_date BETWEEN $P{date1} and $P{date2}
	and hrl1.apv_status = '1'
	and hrl1.leave_code = '001' and hrl1.emp_no = hrl.emp_no),0) +
NVL((select sum(hrl2.count_status)
	from HRLOGLEAVE hrl2
	where hrl2.leave_date BETWEEN $P{date1} and $P{date2}
	and hrl2.apv_status = '1'
	and hrl2.leave_code = '002' and hrl2.emp_no = hrl.emp_no),0) +
NVL((select sum(hrl3.count_status)
	from HRLOGLEAVE hrl3
	where hrl3.leave_date BETWEEN $P{date1} and $P{date2}
	and hrl3.apv_status = '1'
	and hrl3.leave_code = '003' and hrl3.emp_no = hrl.emp_no),0) +
NVL((select sum(hrl4.count_status)
	from HRLOGLEAVE hrl4
	where hrl4.leave_date BETWEEN $P{date1} and $P{date2}
	and hrl4.apv_status = '1'
	and hrl4.leave_code = '004' and hrl4.emp_no = hrl.emp_no),0) +
NVL((select sum(hrl6.count_status)
	from HRLOGLEAVE hrl6
	where hrl6.leave_date BETWEEN $P{date1} and $P{date2}
	and hrl6.apv_status = '1'
	and hrl6.leave_code = '006' and hrl6.emp_no = hrl.emp_no),0) +
NVL((select sum(hrl5.count_status)
	from HRLOGLEAVE hrl5
	where hrl5.leave_date BETWEEN $P{date1} and $P{date2}
	and hrl5.apv_status = '1'
	and hrl5.leave_code in ('005','007') and hrl5.emp_no = hrl.emp_no),0) as sum_all,
NVL((select sum(hll.count_late)
	from hrloglate hll
	where trunc(hll.late_date) BETWEEN  $P{date1} and  $P{date2}
	and hll.count_late = '1'
	and hll.emp_no = hr.emp_no),0) as work_late
FROM
HREMPLOYEE hr  left join HRLOGLEAVE hrl on hr.emp_no = hrl.emp_no and hrl.leave_date BETWEEN $P{date1} and $P{date2} and hrl.apv_status = '1',
mbucfprename mup
WHERE hr.prename_code = mup.prename_code
and hr.emp_status  in ('1','3')
order by hr.emp_no]]>
	</queryString>
