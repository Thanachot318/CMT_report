	<parameter name="coop_id" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT
sum(a.int_amt) as sum_int,
sum(a.tax_amt) as sum_tax,
 a.deptaccount_no,
 b.deptaccount_name,
c.card_person,
d.coop_name
FROM   DPINTTAXSTATEMENT a
join DPDEPTMASTER b on b.deptaccount_no = a.deptaccount_no and b.depttype_code = a.depttype_code
 join      MBMEMBMASTER c on c.member_no = b.member_no and c.membcat_code = b.membcat_code
  join     CMCOOPCONSTANT d on d.coop_id = b.coop_id
WHERE   b.COOP_ID = $P{coop_id} and
a.PRNC_DUEDATE between  $P{start_date} and $P{end_date}
and a.tax_amt >0
and a.due_flag = 1
group by  a.deptaccount_no,
 b.deptaccount_name,
c.card_person,
d.coop_name
order by deptaccount_no]]>
	</queryString>
	<field name="sum_int" class="java.math.BigDecimal"/>
	<field name="sum_tax" class="java.math.BigDecimal"/>
