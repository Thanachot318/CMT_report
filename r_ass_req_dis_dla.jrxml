	<queryString>
		<![CDATA[SELECT
ASSREQMASTER.MEMBER_NO,
MBUCFPRENAME.PRENAME_DESC,
MBMEMBMASTER.MEMB_NAME,
MBMEMBMASTER.MEMB_SURNAME,
MBUCFMEMBGROUP.MEMBGROUP_CODE,
MBUCFMEMBGROUP.MEMBGROUP_DESC,
ASSREQMASTERDET.MONEYTYPE_CODE,
ASSREQMASTERDET.EXPENSE_ACCID,
ASSREQMASTERDET.DEPTACCOUNT_NO,
CMUCFBANK.BANK_DESC,
ASSREQMASTER.DIS_DISDATE,
ASSREQMASTER.REQ_DATE,
assucfdisaster.disaster_desc,
trim(ASSREQMASTER.DIS_DISTYPE)as DISASTER_CODE,
ASSREQMASTER.DIS_DISTYPE,
ASSREQMASTER.ASSISTNET_AMT,

ASSREQMASTER.REMARK,
ASSUCFASSISTTYPEPAY.ASSISTPAY_DESC,
ASSUCFASSISTTYPE.ASSISTTYPE_DESC,
MBUCFMEMBTYPE.MEMBTYPE_DESC,
ASSREQMASTER.ASSISTTYPE_CODE,
ASSREQMASTER.ENTRY_ID,
MBUCFMEMBTYPE.MEMBTYPE_CODE,
ASSREQMASTER.SEND_SYSTEM  ,
CMCOOPMASTER.COOP_NAME,
mbmembmaster.member_date,
case
trunc(months_between(LAST_DAY(TO_DATE(ASSREQMASTER.REQ_DATE)), mbmembmaster.birth_date) / 12) when 0 then '0'
else trunc(months_between(LAST_DAY(TO_DATE(ASSREQMASTER.REQ_DATE)), mbmembmaster.birth_date) / 12)  || '' end as yrs,
case
trunc(mod(months_between(LAST_DAY(TO_DATE(ASSREQMASTER.REQ_DATE)),mbmembmaster.birth_date), 12)) when 0 then '0'
else ' ' || trunc(mod(months_between(LAST_DAY(TO_DATE(ASSREQMASTER.REQ_DATE)), mbmembmaster.birth_date), 12)) || ' ' end as mnths,


case
trunc(months_between(ASSREQMASTER.REQ_DATE,mbmembmaster.member_date) / 12) when 0 then '0'
else trunc(months_between(ASSREQMASTER.REQ_DATE, mbmembmaster.member_date) / 12)  || '' end as yrs_m,
case
trunc(mod(months_between(ASSREQMASTER.REQ_DATE,mbmembmaster.member_date), 12)) when 0 then '0'
else ' ' || trunc(mod(months_between(ASSREQMASTER.REQ_DATE,mbmembmaster.member_date), 12)) || ' ' end as mnths_m,

assconstant.name_1,assconstant.name_2,
ASSREQMASTER.med_startdate,ASSREQMASTER.med_enddate
,(select entry_id from cmreportprocessing where process_id = tp.proc_id) as print_name
,(select full_name from amsecusers where user_name = (select entry_id from cmreportprocessing where process_id = tp.proc_id)  and coop_id = $P{as_coopid} ) as name_pri
,assconstant.name_1,assconstant.name_2
,calage_date
,ASSREQMASTER.req_status
,mbmembmaster.mem_tel
,mbucfprovince.province_desc
,CMUCFBANK.bank_shortname
FROM ASSREQMASTER
left join ASSREQMASTERDET  ON ASSREQMASTERDET.ASSIST_DOCNO = ASSREQMASTER.ASSIST_DOCNO
join  ASSUCFASSISTTYPE on  ASSREQMASTER.ASSISTTYPE_CODE = ASSUCFASSISTTYPE.ASSISTTYPE_CODE
join ASSUCFASSISTTYPEPAY on  ASSREQMASTER.ASSISTPAY_CODE = ASSUCFASSISTTYPEPAY.ASSISTPAY_CODE
and  ASSUCFASSISTTYPE.ASSISTTYPE_CODE = ASSUCFASSISTTYPEPAY.ASSISTTYPE_CODE
join MBMEMBMASTER on  MBMEMBMASTER.MEMBER_NO = ASSREQMASTER.MEMBER_NO
and ASSREQMASTER.COOP_ID = MBMEMBMASTER.COOP_ID
join MBUCFMEMBGROUP on   MBMEMBMASTER.MEMBGROUP_CODE = MBUCFMEMBGROUP.MEMBGROUP_CODE
join MBUCFPRENAME on  MBMEMBMASTER.PRENAME_CODE = MBUCFPRENAME.PRENAME_CODE
join MBUCFMEMBTYPE on   MBMEMBMASTER.MEMBTYPE_CODE = MBUCFMEMBTYPE.MEMBTYPE_CODE
join CMCOOPMASTER on   CMCOOPMASTER.COOP_ID = MBMEMBMASTER.COOP_ID
left join CMUCFBANK  on  ASSREQMASTERDET.EXPENSE_BANK =  CMUCFBANK .BANK_CODE
join CMUCFMONEYTYPE on  ASSREQMASTERDET.MONEYTYPE_CODE = CMUCFMONEYTYPE.MONEYTYPE_CODE
join assconstant on ASSREQMASTER.coop_id = assconstant.coop_id
join  (select max(process_id)  as proc_id, coop_id from cmreportprocessing group by coop_id) tp on tp.coop_id = CMCOOPMASTER.coop_id
left join assucfdisaster on trim(ASSREQMASTER.dis_distype) = trim(assucfdisaster.disaster_code )
left join mbucfprovince on MBMEMBMASTER.province_code = mbucfprovince.province_code

WHERE
(ASSREQMASTER.COOP_ID = $P{as_coopid} ) AND
(ASSREQMASTER.ASSISTTYPE_CODE between  $P{assisttype_start} and  $P{assisttype_end}) AND
(ASSUCFASSISTTYPE.ASSISTTYPE_GROUP = '04' ) AND
(ASSREQMASTER.REQ_STATUS >0 )
 AND ( ASSREQMASTER.REQ_DATE between $P{date1} and  $P{date2})
ORDER BY ASSREQMASTER.ASSISTTYPE_CODE,MBMEMBMASTER.MEMBER_NO,ASSREQMASTER.REQ_DATE]]>
	</queryString>
