	<parameter name="as_eempno" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[select DISTINCT
hr.emp_no,
mp.prename_desc||hr.emp_name||' '||hr.emp_surname as fullname ,
hd.pos_desc,
hdg.deptgrp_desc,
hud.education_desc,
hre.edu_degree,
hre.edu_major,
hr.contain_date,
TRUNC(MONTHS_BETWEEN (SYSDATE,hr.work_date ) /12 )  || ' ปี' || ' ' ||
TRUNC(MOD(MONTHS_BETWEEN(SYSDATE,hr.work_date),12)) || ' เดือน' as age,
(case when hr.emptype_code in ('01','02') then hr.salary_amt else hr.daily_wage end) as salary_amt,
hr.emplevel_code  as emplevel_code,
'ระดับ '||hr.emplevel_code  as emplevel_code_1,
hrc.order_date
from hremployee hr ,
 hremployeeedu hre ,
 hrucfpositionmas hm ,
hrucfpositiondet hd ,
mbucfprename mp,
hrlogchangework hrc ,
hrucfdeptgrp hdg ,
hrucfeducation hud
where hr.prename_code = mp.prename_code
and hr.emp_no = trim(hre.emp_no(+))
and hr.pos_code = hd.pos_code(+)
and hd.pos_mas  = hm.pos_mas(+)
and hr.emp_no = hrc.emp_no(+)
and hr.emplevel_code = hrc.emplevel_code(+)
and hr.deptgrp_code = hdg.deptgrp_code(+)
and hre.education_code = hud.education_code(+)
and hr.coop_id = $P{as_coop_id}
and hr.emp_no  between $P{as_sempno} and $P{as_eempno}
order by hr.emp_no]]>
	</queryString>
