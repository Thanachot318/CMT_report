	<queryString>
		<![CDATA[SELECT
FINSLIP.PAY_TOWHOM,
FINSLIP.ENTRY_DATE,
FINSLIPDET.SLIPITEM_DESC,
FINSLIPDET.ITEM_AMT,
FINSLIP.MEMBER_NO,
FINSLIPDET.CASH_TYPE,
FINSLIPDET.SEQ_NO,
FINSLIP.RECEIVE_DATE,
FINSLIP.REMARK,
FINSLIP.SLIP_NO,
FINSLIPDET.VAT_AMT,
FINSLIP.MEMBER_FLAG,
FINSLIP.RECEIPT_NO
FROM
FINSLIP,
FINSLIPDET
WHERE
( FINSLIP.SLIP_NO = FINSLIPDET.SLIP_NO ) and
( FINSLIP.COOP_ID = FINSLIPDET.COOP_ID ) and
finslip.payment_status <> -9   and
( trim(FINSLIP.SLIP_NO) = $P{as_slipno})
and ( finslip.coop_id = $P{as_coopid})]]>
	</queryString>
