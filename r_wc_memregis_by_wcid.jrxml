	<queryString>
		<![CDATA[select
    wcreqdeposit.wc_id as wc_id,
    wcreqdeposit.member_no  as member_no,
    trim(mbucfprename.prename_desc) as prename_desc,
    wcreqdeposit.deptopen_date as deptopen_date,
    wcreqdeposit.wfbirthday_date as wfbirthday_date,
    trim(wcreqdeposit.deptaccount_name)  as  deptaccount_name,
    trim(wcreqdeposit.deptaccount_sname) as deptaccount_sname,
    wcreqdeposit.card_person as card_person,
    wccontcoop.coop_name
from wcreqdeposit
    left join mbucfprename on wcreqdeposit.prename_code = mbucfprename.prename_code
    left join wccontcoop on wcreqdeposit.coop_id = wccontcoop.coop_id
        and wcreqdeposit.wc_id = wccontcoop.wc_id
        and (wccontcoop.application = $P{as_app} OR $P{as_app} IS NULL)
where
    wcreqdeposit.coop_id = $P{as_coopid}
    and wcreqdeposit.wc_id = $P{as_wcid}
    and trunc(wcreqdeposit.deptopen_date) between to_date($P{start_date}, 'YYYYMMDD') and to_date($P{end_date}, 'YYYYMMDD')
    and wcreqdeposit.approve_status > 0
order by
    wcreqdeposit.deptopen_date]]>
	</queryString>
