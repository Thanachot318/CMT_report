	<queryString>
		<![CDATA[select lncontstatement.loancontract_no ,
lncontstatement.slip_date,
lncontstatement.loanitemtype_code,
lncontmaster.loanrequest_amt,
lncontstatement.period,
lncontstatement.principal_payment,
lncontstatement.interest_payment,
lncontstatement.principal_balance,
lnloantype.loangroup_code,
lncontmaster.loantype_code
from
lncontmaster
left join lncontstatement on lncontmaster.loancontract_no = lncontstatement.loancontract_no
left join lnloantype on lncontmaster.loantype_code = lnloantype.loantype_code
where lncontmaster.member_no = $P{member_no}
and lncontstatement.slip_date between $P{s_date} and $P{e_date}
and lnloantype.loangroup_code in('01')
and lncontstatement.seq_no >= 1
order by lnloantype.loangroup_code,lncontmaster.loantype_code,lncontstatement.loancontract_no , lncontstatement.seq_no]]>
	</queryString>
