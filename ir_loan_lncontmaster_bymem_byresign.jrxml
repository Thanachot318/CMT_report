	<queryString>
		<![CDATA[SELECT
    mb.member_no,
    mb.resign_status,
    ft_getmbname(mb.coop_id, mb.member_no) AS fullname,
    mb.membgroup_code,
    mp.membgroup_desc,
    la.loantype_code AS loantype_code,
    la.loancontract_no AS loancontract_no,
    la.lastcalint_date, la.startcont_date,
    NVL(la.loanapprove_amt,0) AS loanapprove_amt,
    NVL(la.principal_balance,0) AS principal_balance,
    NVL(la.period_payment,0) AS period_payment
FROM mbmembmaster mb
LEFT JOIN mbucfmembgroup mp ON mb.membgroup_code = mp.membgroup_code
LEFT JOIN (
    SELECT
        lnm.coop_id,
        lnm.member_no,
        lnm.loantype_code,
        lnm.loancontract_no,
        lnm.lastcalint_date, lnm.startcont_date,
        NVL(lnm.loanapprove_amt,0) AS loanapprove_amt,
        NVL(lnm.principal_balance,0) AS principal_balance,
        NVL(lnm.period_payment,0) AS period_payment
    FROM lncontmaster lnm
    JOIN lnloantype lnt ON lnt.loantype_code = lnm.loantype_code
    WHERE lnm.principal_balance > 0 and lnm.contract_status > 0
) la ON la.coop_id = mb.coop_id AND la.member_no = mb.member_no
WHERE mb.coop_id = $P{as_scoop}
  AND mb.resign_status like $P{status}
  AND mb.membgroup_code BETWEEN $P{smemgroup_code} AND $P{ememgroup_code}
  AND la.loantype_code BETWEEN $P{sloantype_code} AND $P{eloantype_code}
ORDER BY mb.membgroup_code, mb.member_no, la.loantype_code, la.loancontract_no]]>
	</queryString>
