	<queryString>
		<![CDATA[select lncollmaster.*,cmcoopmaster.*, ( MBUCFPRENAME.PRENAME_DESC || MBMEMBMASTER.MEMB_NAME || '   ' || MBMEMBMASTER.MEMB_SURNAME  ) as Mem_Name,FT_READTBAHT(est_price) as m_bath,
(select tambol_desc from mbucftambol where tambol_code = lncollmaster.pos_tambol) as tambol_desc,
(select district_desc from mbucfdistrict where district_code = lncollmaster.pos_amphur)  as amphur_desc,
(select province_desc from mbucfprovince where province_code = lncollmaster.pos_province)  as province_desc,
(select tambol_desc from mbucftambol where tambol_code = lncollmaster.MRTG_TAMBOL1) as tambol_desc2,
(select district_desc from mbucfdistrict where district_code = lncollmaster.MRTG_AMPHUR1)  as amphur_desc2,
(select province_desc from mbucfprovince where province_code = lncollmaster.MRTG_PROVINCE1)  as province_desc2,
(select tambol_desc from mbucftambol where tambol_code = lncollmaster.autzd_tambol) as tambol_desc3,
(select district_desc from mbucfdistrict where district_code = lncollmaster.autzd_amphur)  as amphur_desc3,
(select province_desc from mbucfprovince where province_code = lncollmaster.autzd_province)  as province_desc3
 from lncollmaster
inner join cmcoopmaster on cmcoopmaster.coop_id = lncollmaster.coop_Id
inner join MBMEMBMASTER on lncollmaster.member_no = MBMEMBMASTER.member_no
inner join MBUCFPRENAME on MBMEMBMASTER.PRENAME_CODE = MBUCFPRENAME.PRENAME_CODE
 where lncollmaster.collmast_no= $P{colmast_no} and lncollmaster.coop_id = $P{coopid}]]>
	</queryString>
