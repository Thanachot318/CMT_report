	<queryString>
		<![CDATA[select mug.membgroup_code , mug.membgroup_desc , count( ym.member_no ) as mem_amt , sum( ym.div_amt ) as div_amt , sum( ym.avg_amt ) as avg_amt,
cm.coop_name as coop_name
from yrdivmaster ym
		join mbmembmaster mb on ym.coop_id = mb.coop_id and ym.member_no = mb.member_no
		join mbucfmembgroup mug on  mb.coop_id = mug.coop_id and mb.membgroup_code = mug.membgroup_code
		join cmcoopconstant cm on ym.coop_id = cm.coop_id
where ym.coop_id = $P{as_coopid}
and trim(ym.div_year) = $P{as_year}
and mb.membgroup_code between $P{as_smembgroup} and $P{as_emembgroup}
group by mug.membgroup_code , mug.membgroup_desc, cm.coop_name
order by mug.membgroup_code]]>
	</queryString>
