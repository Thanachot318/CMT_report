	<queryString>
		<![CDATA[SELECT
 substring(hr.payroll_period,1,4) as t1,
isnull(c.addr_no,'') + ' หมู่' + isnull(c.addr_moo,'') + ' ถนน' + isnull(c.addr_road,'') + ' ตำบล' + isnull(c.addr_tambol,'') + ' อำเภอ' + isnull(c.addr_amphur,'') + ' ' +
isnull(c.addr_province,'') + ' ' + isnull(c.addr_postcode,'') as ADDCCOP,
cm.coop_name as COOP_NAME ,
coop_taxid as COOP_TAXID,
he.emp_no as EMP_NO,
concat(concat(concat(mp.prename_desc,he.emp_name),'  '),he.emp_surname) as EMP_NAME,
concat(concat(concat(concat(concat(concat(concat(concat(substring(he.id_card,1,1),'-'),substring(he.id_card,2,4)),'-'),substring(he.id_card,6,5)),'-'),substring(he.id_card,11,2)),'-'),substring(he.id_card,13,1))  as ID_CRAD,
(case when MBUCFPROVINCE.province_desc = 'กรุงเทพมหานคร'
	 then concat(concat(concat(concat(concat(concat(concat(concat(concat(concat(concat(concat(concat(isnull(he.adr_no,' -'),'   '),'แขวง '),isnull(mbucftambol.tambol_desc,' -')),'  '),'เขต '),isnull(MBUCFDISTRICT.district_desc,' -')),'  '),'จังหวัด '),isnull(MBUCFPROVINCE.province_desc,' -')),'  '),'รหัสไปรษณีย์'),' '),isnull(he.adr_postcode,' -'))
else concat(concat(concat(concat(concat(concat(concat(concat(concat(concat(concat(concat(concat(isnull(he.adr_no,' -'),'   '),'ตำบล '),isnull(mbucftambol.tambol_desc,' -')),'  '),'อำเภอ '),isnull(MBUCFDISTRICT.district_desc,' -')),'  '),'จังหวัด '),isnull(MBUCFPROVINCE.province_desc,' -')),'  '),'รหัสไปรษณีย์'),' '),isnull(he.adr_postcode,' -')) end ) as ADR_DESS,
substring(($P{as_period}),3,2) as YEAR,
substring($P{as_period},1,4) as YEAR2,
(SELECT sum(hd.ITEM_AMT)
from HRPAYROLLDET hd,hrpayroll hr
where hr.PAYROLLSLIP_NO = hd.PAYROLLSLIP_NO
and hd.SALITEM_CODE = 'P01'
and substring(hr.payroll_period,1,4) = substring($P{as_period},1,4)
and (hr.EMP_NO) between  $P{as_sempno}  and  $P{as_eempno}
and he.emp_no = hr.EMP_NO
) + isnull(hrbonus.bonus_amt_tax,0) as P01,
dbo.ft_readthaibaht((SELECT sum(hd.ITEM_AMT)
from HRPAYROLLDET hd,hrpayroll hr
where hr.PAYROLLSLIP_NO = hd.PAYROLLSLIP_NO
and hd.SALITEM_CODE = 'P01'
and substring(hr.payroll_period,1,4) = substring($P{as_period},1,4)
and (hr.EMP_NO) between  $P{as_sempno}  and  $P{as_eempno}
and he.emp_no = hr.EMP_NO
) + isnull(hrbonus.bonus_amt_tax,0)) as P01_THAI,
(SELECT sum(hd.ITEM_AMT)
from HRPAYROLLDET hd,hrpayroll hr
where hr.PAYROLLSLIP_NO = hd.PAYROLLSLIP_NO
and hd.SALITEM_CODE = 'P02'
and substring(hr.payroll_period,1,4) =  substring($P{as_period},1,4)
and (hr.EMP_NO) between $P{as_sempno}  and  $P{as_eempno}
and he.emp_no = hr.EMP_NO
) as P02,
(SELECT sum(hd.ITEM_AMT)
from hrpayroll hr join HRPAYROLLDET hd on hr.PAYROLLSLIP_NO = hd.PAYROLLSLIP_NO
left join hrucfsalaryitem hus on hd.salitem_code = hus.salitem_code
where hus.sign_flag = 1
and substring(hr.payroll_period,1,4) = substring($P{as_period},1,4)
and (hr.EMP_NO) between $P{as_sempno}  and  $P{as_eempno}
and he.emp_no = hr.EMP_NO
) + isnull(hrbonus.bonus_amt,0) as R01,
(select concat(concat(concat(m.prename_desc,h.emp_name),'  '),h.emp_surname)
from hremployee h , mbucfprename m
where h.prename_code = m.prename_code
and h.emp_no = $P{as_emp_no}
 ) as FNAME
FROM
HREMPLOYEE he
INNER JOIN MBUCFPRENAME mp ON he.PRENAME_CODE = mp.PRENAME_CODE
FULL OUTER JOIN hrpayroll hr ON he.emp_no = hr.EMP_NO
LEFT JOIN   MBUCFDISTRICT MBUCFDISTRICT on he.adn_amphur =  MBUCFDISTRICT.district_code
and he.adn_province = MBUCFDISTRICT.province_code
LEFT JOIN  MBUCFPROVINCE MBUCFPROVINCE on he.adn_province = MBUCFPROVINCE.province_code
LEFT JOIN mbucftambol mbucftambol on he.adr_tambol = mbucftambol.tambol_code
LEFT JOIN hrbonus hrbonus on he.emp_no = hrbonus.emp_no and hrbonus.year_bonus = cast(cast(substring($P{as_period},1,4) as int) - 1 as varchar)
LEFT JOIN cmcoopconstant cm on he.coop_id  = cm.coop_id
LEFT JOIN cmcoopmaster c on he.coop_id  = c.coop_id
where hr.coop_id = $P{as_coopid}
and substring(hr.payroll_period,1,4) =  substring($P{as_period},1,4)
and (hr.EMP_NO) between  $P{as_sempno}  and  $P{as_eempno}
and he.emp_status = 1
group by he.emp_no , mp.prename_desc , he.emp_name , he.emp_surname,he.id_card,he.adr_no,mbucftambol.tambol_desc,he.adr_postcode,MBUCFDISTRICT.district_desc,MBUCFPROVINCE .province_desc,c.addr_no,c.addr_moo,c.addr_road,c.addr_tambol,c.addr_amphur,c.addr_province,c.addr_postcode,bonus_amt,bonus_amt_tax,
cm.coop_name,
coop_taxid,hr.payroll_period
ORDER BY
he.emp_no ASC]]>
	</queryString>
