	<queryString>
		<![CDATA[select
case when $P{status} = '%' then 'ทั้งหมด'
     when $P{status} = '8' then 'รออนุมัติ'
     when $P{status} = '1' then 'อนุมัติ'
     when $P{status} = '-1' then 'ไม่อนุมัติ'
     when $P{status} = '-9' then 'ยกเลิก'
     when $P{status} = '2' then 'สิทธิ์กู้'
     when $P{status} = '3' then 'รอตรวจสอบ'
else '' end as status,
lnreqloan.loantype_code,
lnreqloan.loanrequest_docno,
lnreqloan.loanrequest_status,
lnreqloan.member_no,
mbmembmaster.membgroup_code,
mbucfmembgroup.membgroup_desc,
mbmembmaster.position_desc,
lnreqloan.loanrequest_date,
ft_getgrpname(lnreqloan.coop_id, mbmembmaster.membgroup_code) as grp_desc,
ft_getmbname(lnreqloan.coop_id, lnreqloan.member_no) as fullname,

replace(to_char(lnreqloan.birth_age), '.', '/') as birth_age,
replace(to_char(lnreqloan.work_age), '.', '/') as work_age,
replace(to_char(lnreqloan.member_age), '.', '/') as member_age,

lnreqloan.salary_bal,
lnreqloan.share_balance,
lnreqloan.share_periodvalue,
lnreqloan.loancredit_amt,
lnreqloan.loanpermiss_amt ,
lnreqloan.loanrequest_amt ,

lnreqloan.period_payamt ,
lnreqloan.period_payment,

lnreqloan.salary_amount,
lnreqloan.incomemonth_fix,
lnreqloan.incomemonth_other,
lnreqloan.incomemonth_fix + lnreqloan.incomemonth_other as income_other,

lnreqloan.paymonth_coop,
lnreqloan.paymonth_other,
lnreqloan.paymonth_exp,
lnreqloan.paymonth_exp + lnreqloan.paymonth_other as pay_other,

COALESCE(lnreqloan.sum_clramt,0) as clr_amt,
COALESCE(lnreqloan.sum_intclramt,0) as intclr_amt,
COALESCE(lnreqloan.sum_clrotheramt,0) as clrother_amt,
COALESCE(lnreqloan.sum_premiumamt,0) as clrins_amt,

lnreqloan.recvestimate_amt,

lnreqloan.entry_id,
cmucfmoneytype.moneytype_desc,
cmucfbank.bank_desc,
lnreqloan.expense_bank,
lnreqloan.remark,
mbucfprovince.province_code,
mbucfprovince.province_desc
from lnreqloan
left join mbmembmaster on lnreqloan.member_no = mbmembmaster.member_no
left join mbucfmembgroup on mbmembmaster.membgroup_code = mbucfmembgroup.membgroup_code
left join cmucfmoneytype on lnreqloan.expense_code = cmucfmoneytype.moneytype_code
left join cmucfbank on lnreqloan.expense_bank = cmucfbank.bank_code
left join mbucfprovince on mbucfprovince.province_code = mbmembmaster.province_code
where
trim(lnreqloan.coop_id) = $P{as_scoop}
and lnreqloan.loanrequest_date between  $P{adtm_start} and $P{adtm_end}
and trim(lnreqloan.loantype_code) between $P{sloantype_code} and $P{eloantype_code}
and trim(lnreqloan.loanrequest_status) like $P{status}
and (lnreqloan.expense_bank like $P{bank_code} or lnreqloan.expense_bank is null)
and (lnreqloan.expense_code like $P{money_code} or lnreqloan.expense_code is null)
and lnreqloan.loanrequest_amt between $P{smoneypay_amt} and $P{emoneypay_amt}
order by mbmembmaster.membgroup_code, mbucfprovince.province_code, lnreqloan.expense_code, lnreqloan.member_no]]>
	</queryString>
