	<queryString>
		<![CDATA[SELECT  ACCMASTER.ACCOUNT_ID ,
 lpad( '          ' , 10 *( account_type_id - 1 ) ) || ACCMASTER.ACCOUNT_ID || '  ' || ACCMASTER.ACCOUNT_NAME as account_name,
         ACCCNTSECTION.SECTION_DESC ,
        ACCMASTER.SECTION_ID ,
        ACCMASTER.ACCOUNT_SORT ,
        ACCMASTER.ACCOUNT_TYPE_ID ,
        0 as select_row,
         CMCOOPMASTER.COOP_NAME
  FROM ACCMASTER ,
         ACCCNTSECTION ,
        CMCOOPMASTER
  WHERE ( ACCMASTER.SECTION_ID = ACCCNTSECTION.SECTION_ID ) and
     ( ACCMASTER.COOP_ID = CMCOOPMASTER.COOP_ID ) and
     ( ( ACCMASTER.COOP_ID =$P{as_coopid} ) )
ORDER BY ACCMASTER.ACCOUNT_ID          ASC,           ACCMASTER.SECTION_ID          ASC]]>
