	<queryString>
		<![CDATA[SELECT
f.COOP_ID,
f.ENTRY_ID ,
f.OPERATE_DATE ,
f.ITEM_STATUS ,
f.AMOUNT_BALANCE ,
a.FULL_NAME
FROM
FINTABLEUSERMASTER f
LEFT JOIN AMWEBUSERS a ON f.ENTRY_ID = a.USER_NAME
WHERE
f.COOP_ID = $P{as_coopid} AND
f.OPERATE_DATE = $P{adtm_date}]]>
	</queryString>
