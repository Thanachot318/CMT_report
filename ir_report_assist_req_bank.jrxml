	<queryString>
		<![CDATA[SELECT AR.MEMBER_NO,MB.PRENAME_CODE,MB.MEMB_NAME,MB.MEMB_SURNAME,AR.APPROVE_DATE,AR.ASSIST_AMT,AR.ASSISTTYPE_CODE,
AR.MONEYTYPE_CODE ,AR.EXPENSE_BANK,AR.EXPENSE_BRANCH,AR.EXPENSE_ACCID,AR.DEPTACCOUNT_NO,CMUCFBANK.BANK_DESC,CMUCFBANKBRANCH.BRANCH_NAME,
(SELECT UT.ASSISTTYPE_DESC FROM ASSUCFASSISTTYPE UT WHERE UT.ASSISTTYPE_CODE=AR.ASSISTTYPE_CODE) ASSISTTYPE_DESC,
(SELECT MP.PRENAME_DESC FROM MBUCFPRENAME MP WHERE MB.PRENAME_CODE=MP.PRENAME_CODE)PRENAME_DESC,
(SELECT CC.COOP_NAME FROM CMCOOPCONSTANT CC WHERE MB.COOP_ID=CC.COOP_ID) COOP_NAME
,AR.REQ_DATE
FROM ASSREQMASTER AR
JOIN MBMEMBMASTER MB ON AR.MEMBER_NO=MB.MEMBER_NO AND AR.COOP_ID = MB.COOP_ID
LEFT JOIN  CMUCFBANK ON AR.EXPENSE_BANK = CMUCFBANK.BANK_CODE
LEFT JOIN  CMUCFBANKBRANCH ON CMUCFBANKBRANCH.BANK_CODE = CMUCFBANK.BANK_CODE
AND AR.EXPENSE_BRANCH = CMUCFBANKBRANCH.BRANCH_ID
WHERE AR.REQ_DATE BETWEEN $P{date1} AND $P{date2}
AND AR.REQ_STATUS  in (1,8)
AND AR.ASSISTTYPE_CODE BETWEEN  $P{assisttype_start} AND $P{assisttype_end} AND AR.REQ_STATUS = 1 AND AR.COOP_ID =$P{as_coopid}
ORDER BY AR.ASSISTTYPE_CODE,AR.REQ_DATE]]>
	</queryString>
