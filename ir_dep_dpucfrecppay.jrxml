	<queryString>
		<![CDATA[select
    coop_id,
    recppaytype_code,
    recppaytype_desc,
    deptitem_group

from dpucfrecppaytype
where active_flag = 1
order by
    recppaytype_flag desc,
    case
        when deptitem_group = 'OPN' then 1
        when deptitem_group = 'DEP' then 2
        when deptitem_group = 'WID' then 3
        when deptitem_group = 'CLS' then 4
        when deptitem_group = 'INT' then 5
        when deptitem_group = 'TAX' then 6
        when deptitem_group = 'FEE' then 7
        when deptitem_group = 'ERR' then 8
        else 0
    end,
    recppaytype_code asc]]>
	</queryString>
