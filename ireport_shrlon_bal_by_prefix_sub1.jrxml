	<queryString>
		<![CDATA[select
(case when mb.membcat_code = '10' then 'สมาชิกปกติ'
when mb.membcat_code = '20' then 'สมาชิกสมทบ'
when mb.membcat_code = '30' then 'บุคคลอื่น'
else '' end )as memb_type ,
count(mb.member_no) as count_memb ,
sum(case when mb.sex = 'M' then 1 else 0 end) as Mcount_memb,
sum(case when mb.sex = 'F' then 1 else 0 end) as Fcount_memb,
sum(case when sh.sharestk_amt > 0 then sh.sharestk_amt * st.unitshare_value  else 0 end) as shr_bal,
sum(case when sh.sharestk_amt <= 0 then 1 else 0 end) as not_shr
from  mbmembmaster mb
left join shsharemaster sh on mb.coop_id = sh.coop_id and mb.member_no = sh.member_no
join shsharetype st on sh.sharetype_code = st.sharetype_code
where
mb.membgroup_code between $P{as_sgroup} and $P{as_egroup}
and mb.resign_status = 0
group by mb.membcat_code
order by mb.membcat_code]]>
	</queryString>
