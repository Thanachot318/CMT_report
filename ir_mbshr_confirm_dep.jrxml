	<parameter name="adtm_date" class="java.util.Date">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[select
yrs.bizzaccount_no,
yrs.balance_amt,
yrs.from_system,
sum(yrs.balance_amt) as sum_balance_amt,
typ.depttype_desc,
dep.depttype_code
from yrconfirmstatement yrs
left join dpdeptmaster dep on rtrim(dep.deptaccount_no) = rtrim(yrs.bizzaccount_no)
left join dpucfdepttype typ on rtrim(typ.depttype_code) = rtrim(dep.depttype_code)
where
yrs.member_no = $P{as_memberno}
and yrs.balance_date = $P{adtm_date}
and yrs.from_system = 'DEP'
group by
yrs.bizztype_code,
yrs.bizzaccount_no,
yrs.balance_amt,
yrs.from_system,
typ.depttype_desc,
dep.depttype_code
ORDER BY yrs.from_system]]>
	</queryString>
