		<![CDATA[select
cheque_no,
 date_onchq,
(substr(to_char( date_onchq , 'dd' ), 1,1)||'     '||substr(to_char( date_onchq , 'dd' ), 2,1) ||'     '|| substr(to_char( date_onchq , 'mm' ), 1,1)||'     '||substr(to_char( date_onchq , 'mm' ), 2,1)||'     '|| substr(to_char( to_number( to_char( date_onchq , 'yyyy' ) ) + 543 ),1,1)
||'     '|| substr(to_char( to_number( to_char( date_onchq , 'yyyy' ) ) + 543 ),2,1)||'     '|| substr(to_char( to_number( to_char( date_onchq , 'yyyy' ) ) + 543 ),3,1)||'     '|| substr(to_char( to_number( to_char( date_onchq , 'yyyy' ) ) + 543 ),4,1) ) as chqdate_dormat,

substr(to_char( date_onchq , 'dd' ), 1,1) as d1,
substr(to_char( date_onchq , 'dd' ), 2,1)  as d2,
substr(to_char( date_onchq , 'mm' ), 1,1) as m1,
substr(to_char( date_onchq , 'mm' ), 2,1) as m2,
substr(to_char( to_number( to_char( date_onchq , 'yyyy' ) ) + 543 ),1,1) as y1,
substr(to_char( to_number( to_char( date_onchq , 'yyyy' ) ) + 543 ),2,1) as y2,
substr(to_char( to_number( to_char( date_onchq , 'yyyy' ) ) + 543 ),3,1) as y3,
substr(to_char( to_number( to_char( date_onchq , 'yyyy' ) ) + 543 ),4,1)   as y4
, trim(to_whom)
,item_amt
, trim(remark)
,'***'||trim(to_char(DECODE(item_amt,0,null,item_amt),'99G999G999G999D00'))||'***'
,'***'||trim(ft_readtbaht(item_amt) )||'***' as thaibathamt


 from FINCHQEUESTATEMENT
where trim(chequebook_no) =  trim($P{as_chequebookno})
and chqeue_status <> -9
and trim(cheque_no) = trim($P{as_chequeno})
and trim(bank_code)  = trim($P{as_bankcode})
and trim(bank_branch)  = trim($P{as_bankbranch})]]>
